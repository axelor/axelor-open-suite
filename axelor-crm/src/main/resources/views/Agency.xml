<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">

  <grid name="agency-grid" title="Agencies" model="com.axelor.apps.crm.db.Agency">
    <field name="name"/>
  </grid>

  <form name="agency-form" title="Agency" model="com.axelor.apps.crm.db.Agency">

    <panel colSpan="12">
      <field name="name"/>
      <field name="memberSet" widget="TagSelect" colSpan="12"/>
    </panel>

  </form>

</object-views>

<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.4.xsd">


  <!-- Nomenclature : * name : "interfaceName" + "fieldName" + ".select" -->

  <selection name="crm.event.related.to.select">
    <option value="com.axelor.apps.crm.db.Lead">Lead</option>
    <option value="com.axelor.apps.crm.db.Opportunity" data-grid="opportunity-grid"
      data-form="opportunity-form">Opportunity</option>
    <option value="com.axelor.apps.crm.db.Event">Event</option>
  </selection>

  <selection name="crm.event.priority.select">
    <option value="1">Low</option>
    <option value="2">Normal</option>
    <option value="3">High</option>
    <option value="4">Urgent</option>
  </selection>

  <selection id="crm.icalendar.event.type.select" name="icalendar.event.type.select">
    <option value="5">Note</option>
  </selection>

  <selection name="crm.event.call.type.select">
    <option value="1">Inbound</option>
    <option value="2">Outbound</option>
  </selection>

  <selection name="crm.event.status.select">
    <option value="1" color="blue">Planned</option>
    <option value="2" color="green">Realized</option>
    <option value="3" color="red">Canceled</option>
  </selection>

  <selection name="crm.event.attendee.status.select">
    <option value="1">Non-participant</option>
    <option value="2">Optional participant</option>
    <option value="3">Required participant</option>
  </selection>

  <selection name="crm.event.reminder.type.select">
    <option value="1">Email</option>
  </selection>

  <selection name="crm.event.reminder.mode.select">
    <option value="1">Before start date</option>
    <option value="2">At specific date</option>
  </selection>

  <selection name="crm.eventReminder.duration.type.select">
    <option value="1">Minutes</option>
    <option value="2">Hours</option>
    <option value="3">Days</option>
    <option value="4">Weeks</option>
  </selection>

  <selection name="crm.eventReminder.assign.to.select">
    <option value="1">For me</option>
    <option value="3">For all</option>
  </selection>

  <selection name="icrm.batch.action.select">
    <option value="21">Event reminder</option>
  </selection>

  <selection name="crm.target.configuration.period.type.select">
    <option value="0">None</option>
    <option value="1">Monthly</option>
    <option value="2">Weekly</option>
    <option value="3">Daily</option>
  </selection>

  <selection name="message.related.to.select" id="crm.message.related.to.select">
    <option value="com.axelor.apps.crm.db.Lead" data-grid="lead-grid" data-form="lead-form">Lead</option>
    <option value="com.axelor.apps.crm.db.Opportunity" data-grid="opportunity-grid"
      data-form="opportunity-form">Opportunity</option>
    <option value="com.axelor.apps.crm.db.Event" data-grid="event-grid"
      data-form="event-form">Event</option>
  </selection>

  <selection name="crm.event.recurrence.type">
    <option value="1">Every day</option>
    <option value="2">Every week</option>
    <option value="3">Every month</option>
    <option value="4">Every year</option>
  </selection>

  <selection name="crm.event.recurrence.end.type">
    <option value="1">After n repetitions</option>
    <option value="2">At the date</option>
  </selection>

  <selection name="crm.event.recurrence.month.repeat.type">
    <option value="1">Day of month</option>
    <option value="2">Day of week</option>
  </selection>

  <selection name="meta.schedule.job.select" id="crm.meta.schedule.job.select">
    <option value="com.axelor.apps.crm.job.EventReminderJob">com.axelor.apps.crm.job.EventReminderJob</option>
  </selection>

  <selection name="meta.schedule.batch.service.select"
    id="crm.meta.schedule.batch.service.select">
    <option value="com.axelor.apps.crm.service.batch.CrmBatchService">com.axelor.apps.crm.service.batch.CrmBatchService</option>
  </selection>

  <selection name="crm.convert.lead.wizard.lead.to.partner.select">
    <option value="1">Create new partner</option>
    <option value="2">Select existing partner</option>
  </selection>

  <selection name="crm.convert.lead.wizard.lead.to.contact.select">
    <option value="0">No contact creation</option>
    <option value="1">Create new contact</option>
    <option value="2">Select existing contact</option>
  </selection>

  <selection name="icalendar.synchronization.select"
    id="crm.icalendar.synchronization.select">
    <option value="Event">CRM events</option>
  </selection>

  <selection name="crm.app.crm.assignable.user.select">
    <option value="0">All users</option>
    <option value="1">Some user groups</option>
  </selection>

  <selection name='trace.back.origin.select' id="trace.back.origin.select.crm">
    <option value="crm">CRM</option>
  </selection>

  <selection name="sequence.generic.code.select" id="crm.sequence.generic.code.select">
    <option value="opportunity">Opportunity</option>
  </selection>

  <selection name="batch.batch.type.select" id="crm.batch.batch.type.select">
    <option value="400">CRM batch</option>
  </selection>

  <selection name="crm.lead.size.select">
    <option value="1">1 to 2</option>
    <option value="2">3 to 5</option>
    <option value="3">6 to 9</option>
    <option value="11">10 to 19</option>
    <option value="12">20 to 49</option>
    <option value="21">50 to 99</option>
    <option value="22">100 to 199</option>
    <option value="31">200 to 249</option>
    <option value="32">250 to 499</option>
    <option value="41">500 to 999</option>
    <option value="42">1000 to 1999</option>
    <option value="51">2000 to 4999</option>
    <option value="52">5000 to 9999</option>
    <option value="53">Over 10000</option>
  </selection>

  <selection name="crm.reporting.type.select">
    <option value="com.axelor.apps.crm.db.Lead">Lead</option>
    <option value="com.axelor.apps.base.db.Partner">Partner</option>
  </selection>

  <selection id="crm.research.model.selection" name="research.model.selection">
    <option value="com.axelor.apps.crm.db.Lead">Lead</option>
    <option value="com.axelor.apps.crm.db.Event">Event</option>
    <option value="com.axelor.apps.crm.db.Opportunity">Opportunity</option>
  </selection>

</object-views>

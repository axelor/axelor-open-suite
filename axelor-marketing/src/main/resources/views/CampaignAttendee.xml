  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">

  <grid name="campaign-attendee-grid" title="Attendees"
    model="com.axelor.apps.marketing.db.CampaignAttendee">
    <field name="relatedToSelect" widget="RefSelect" x-related="relatedToSelectId"/>
  </grid>

  <form name="campaign-attendee-form" title="Attendee"
    model="com.axelor.apps.marketing.db.CampaignAttendee" width="large">

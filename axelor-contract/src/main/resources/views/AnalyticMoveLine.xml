<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.3.xsd">
	
    <form name="analytic-move-line-form" title="Analytic Move Line" model="com.axelor.apps.account.db.AnalyticMoveLine" extension="true" id="contract-analytic-move-line-form">
       
       <extend target="//field[@name='moveLine']">
        <insert position="after">
           <field name="contractLine" if="__config__.app.isApp('contract')" colSpan="12" requiredIf="typeSelect == 4" form-view="contract-line-form" grid-view="contract-line-grid" showIf="contractLine || typeSelect == 4"/>
        </insert>
       </extend>

    </form>
    
</object-views>

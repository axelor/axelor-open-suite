<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_7.0.xsd">

  <module name="crm" package="com.axelor.apps.crm.db"/>

  <entity name="Opportunity">

    <boolean name="contractGenerated" title="Contract generated" default="false"/>
    <one-to-many name="contractList" ref="com.axelor.apps.contract.db.Contract"
      title="Contracts" mappedBy="opportunity"/>
  </entity>

</domain-models>

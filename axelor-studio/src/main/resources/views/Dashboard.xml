<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.3.xsd">

  <dashboard name="dasbhoard-wkf" title="Workflow dashboard">
    <dashlet action="chart:chart.wkf.tracking.status.per.month" title="Status per month"
      height="350"/>
    <dashlet action="chart:chart.wkf.tracking.status.per.day" title="Status per day"
      height="350"/>
    <dashlet action="chart:chart.wkf.tracking.time.spent.per.status"
      title="Time spent per status (hours)" height="350"/>
    <dashlet action="chart:chart.wkf.tracking.time.spent.per.status.per.user"
      title="Time spent per status per user (hours)" height="350"/>
  </dashboard>

</object-views>
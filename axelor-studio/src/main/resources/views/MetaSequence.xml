  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_4.1.xsd">
  
  <grid name="meta-sequence-grid" title="Sequences" model="com.axelor.meta.db.MetaSequence">
      <field name="name" />
      <field name="prefix" />
      <field name="suffix" />

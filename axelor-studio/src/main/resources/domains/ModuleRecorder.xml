<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_4.1.xsd">

  <module name="studio" package="com.axelor.studio.db" />
  
  <entity name="ModuleRecorder" cachable="true">
    <![CDATA[
    This class stores the preferences to be used for the recording(into adk module) of the custom development done.
    Also it stores the log generated by the recording process. 
    ]]>
    <string name="logText" title="Log" large="true" />
    <boolean name="updateServer" default="true" />
    <boolean name="autoCreate" title="Automatic create" />
    <boolean name="allViewUpdate" title="Update all views" />
  </entity>
    
    
 </domain-models>
<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">
  
  <module name="studio" package="com.axelor.studio.db" />
  
  <entity name="JsonCreator" cachable="true">
  	 <many-to-one name="metaModel" title="Object" ref="com.axelor.meta.db.MetaModel" />
  	 <many-to-one name="metaField" title="Json field" ref="com.axelor.meta.db.MetaField" />
  	 <one-to-many name="metaJsonFields" ref="com.axelor.meta.db.MetaJsonField" mappedBy="jsonCreator"/>
  </entity>
  
</domain-models>

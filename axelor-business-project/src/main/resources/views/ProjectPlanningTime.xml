  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.0.xsd">

  <form name="project-planning-time-form" id="business-project-project-planning-time-form"
    title="Planned time" model="com.axelor.apps.project.db.ProjectPlanningTime" extension="true">
    <extend target="/">
      <attribute name="onSave" value="action-project-planning-time-method-update-event"/>
    </extend>
  </form>

  <action-method name="action-project-planning-time-method-update-event">
    <call
      class="com.axelor.apps.businessproject.web.ProjectPlanningTimeBusinessProjectController"
      method="updateEvent"/>
  </action-method>
</object-views>
<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.4.xsd">

  <form id="business-project-project-version-form" name="project-version-form"
    title="Project.version" model="com.axelor.apps.project.db.ProjectVersion" extension="true">

    <extend target="//panel[@name='mainPanel']">
      <insert position="after">
        <panel if="__config__.app.isApp('business-project')"
          if-module="axelor-business-project">
          <panel-dashlet name="taskDashletPanel" title="Ticket associated"
            action="action-dashlet-version-view-show-task" colSpan="12"/>
        </panel>
      </insert>
    </extend>
  </form>
</object-views>
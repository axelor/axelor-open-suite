    css="dashlet-no-dropdown">
    <dataset type="rpc">com.axelor.apps.businessproject.web.ProjectTaskController:getProjectTaskFinancialReportingData</dataset>
    <template>
      <![CDATA[
        <>
        	<Box ms={3}>
        		<h1>
        			<b>{first.currencySymbol}</b> {first.turnover}
        		</h1>
        	</Box>
        </>
      ]]>
    </template>
  </custom>


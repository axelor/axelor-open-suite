<?xml version="1.0" ?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.2.xsd">

	<module name="business-project" package="com.axelor.apps.businessproject.db"/>
	
	<entity name="ProjectFolder">
  		<string name="name"/>
  		<string name="description" large="true"/>
 	 	<many-to-many name="projectSet" title="Projects" ref="com.axelor.apps.project.db.Project" mappedBy="projectFolderSet"/>
	</entity>
	
</domain-models>
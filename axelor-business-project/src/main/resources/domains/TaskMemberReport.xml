<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_7.4.xsd">

  <module name="business-project" package="com.axelor.apps.businessproject.db"/>

  <entity name="TaskMemberReport">
    <many-to-one name="taskReport" ref="com.axelor.apps.businessproject.db.TaskReport"
      title="Task Report"/>
    <many-to-one name="employee" ref="com.axelor.auth.db.User" title="Employee"
      required="true"/>
    <many-to-one name="task" ref="com.axelor.apps.project.db.ProjectTask" title="Task"/>
    <datetime name="startTime" title="Start Time"/>
    <datetime name="endTime" title="End Time"/>
    <decimal name="breakTime" title="Break Time (hours)" precision="5" scale="2"/>
    <decimal name="totalWorkHours" title="Work Hours" precision="6" scale="2"/>
    <many-to-many name="activities"
      ref="com.axelor.apps.project.db.ProjectTaskCategory" title="Activities"/>
  </entity>

</domain-models>
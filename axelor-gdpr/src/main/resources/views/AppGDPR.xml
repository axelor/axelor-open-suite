<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

  <form title="App GDPR" name="app-gdpr-config-form" model="com.axelor.apps.base.db.AppGdpr"
    canDelete="false" canNew="false">
    <panel name="configPanel" title="Configurations">
      <field name="maxRespondingDays"/>

      <field name="anonymizer" colSpan="12"/>
      <field name="requestOrigin" colSpan="12"/>
      <field name="requestSearchConfig" colSpan="12"/>
    </panel>
  </form>
</object-views>
<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

  <grid title="GDPR Erasure Log" name="gdpr-erasure-log-grid"
    model="com.axelor.apps.gdpr.db.GDPRErasureLog">
    <field name="modelLog"/>
    <field name="numberOfrecords"/>
  </grid>

  <form title="GDPR Erasure Log" name="gdpr-erasure-log-form"
    model="com.axelor.apps.gdpr.db.GDPRErasureLog">
    <panel name="mainPanel">
      <field name="modelLog"/>
      <field name="numberOfrecords"/>
      <field name="gdprErasureResponse"/>
    </panel>
  </form>
</object-views>
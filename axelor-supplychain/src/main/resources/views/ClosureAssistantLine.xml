<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">


  <action-group
    id="supplychain-account-closure-assistant-line-open-view-link-to-action-group"
    name="account-closure-assistant-line-open-view-link-to-action-group">
    <action name="save"/>
    <action name="action-closure-assistant-line-validate-fiscal-year-closure"
      if="actionSelect != 1"/>
    <action name="account-closure-assistant-line-open-view-link-to-action-method"
      if="actionSelect != 1"/>
    <action name="supplychain-account-closure-assistant-line-open-view-link-to-action-method"
      if="actionSelect == 1"/>
  </action-group>


  <action-method
    name="supplychain-account-closure-assistant-line-open-view-link-to-action-method">
    <call class="com.axelor.apps.supplychain.web.ClosureAssistantSupplychainController"
      method="openViewLinkToAction"/>
  </action-method>

</object-views>

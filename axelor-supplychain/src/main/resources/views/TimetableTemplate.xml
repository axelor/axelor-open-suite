<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">
    
	<grid name="timetable-template-grid" title="Timetable templates" model="com.axelor.apps.supplychain.db.TimetableTemplate" orderBy="name">
		<field name="code"/>
		<field name="name"/>		
	</grid>
	
	<form name="timetable-template-form" title="Timetable template" model="com.axelor.apps.supplychain.db.TimetableTemplate">
		<panel name="mainPanel">
			<field name="name" colSpan="8"/>
			<field name="code" colSpan="4"/>
			<field name="timetableTemplateLineList" grid-view="timetable-template-line-grid" form-view="timetable-template-line-form" colSpan="12"/>
		</panel>
	</form>
	
</object-views>
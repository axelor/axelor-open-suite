<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.3.xsd">

	<grid name="mrp-family-grid" title="MRP families" model="com.axelor.apps.supplychain.db.MrpFamily">
		<field name="code" />
		<field name="name" />
		<field name="dayNb" />
	</grid>

	<form name="mrp-family-form" title="MRP family" model="com.axelor.apps.supplychain.db.MrpFamily">
		<panel title="mainPanel">
			<field name="code" />
			<field name="name" required="true"/>
			<field name="dayNb" />
		</panel>
	</form>

</object-views>

<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">

  <grid model="com.axelor.apps.supplychain.db.PartnerSupplychainLinkType"
    title="Supply chain links type" editable="true" name="partner-supplychain-link-type-grid">
    <field name="typeSelect"/>
    <field name="link1Name"/>
    <field name="link2Name"/>
  </grid>


  <form model="com.axelor.apps.supplychain.db.PartnerSupplychainLinkType"
    title="Supply chain type" name="partner-supplychain-link-type-form">
    <panel>
      <field name="typeSelect" colSpan="6"/>
      <field name="link1Name"/>
      <field name="link2Name"/>
    </panel>
  </form>

</object-views>

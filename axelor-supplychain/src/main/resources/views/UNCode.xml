<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.4.xsd">

  <grid name="un-code-grid" title="UN codes" model="com.axelor.apps.supplychain.db.UNCode">
    <field name="code"/>
    <field name="codeDescription"/>
    <field name="security"/>
  </grid>

  <form name="un-code-form" title="UN code" model="com.axelor.apps.supplychain.db.UNCode">
    <panel name="mainPanel">
      <field name="code"/>
      <field name="codeDescription" colSpan="12"/>
      <field name="security" colSpan="12"/>
    </panel>
  </form>

</object-views>

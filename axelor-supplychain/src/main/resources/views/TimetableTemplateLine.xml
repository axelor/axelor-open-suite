<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

  <grid name="timetable-template-line-grid" title="Timetable template lines"
    model="com.axelor.apps.supplychain.db.TimetableTemplateLine" orderBy="paymentCondition.id">
    <field name="paymentCondition"/>
    <field name="percentage"/>
  </grid>

  <form name="timetable-template-line-form" title="Timetable template line"
    model="com.axelor.apps.supplychain.db.TimetableTemplateLine">
    <panel name="mainPanel">
      <field name="paymentCondition"/>
      <field name="percentage"/>
    </panel>
  </form>

</object-views>
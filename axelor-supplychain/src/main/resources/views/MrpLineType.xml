<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_4.0.xsd">
	
    <grid name="mrp-line-type-grid" model="com.axelor.apps.supplychain.db.MrpLineType" title="MRP Types" orderBy="sequence">
        <hilite if="[5,8,9].indexOf(elementSelect) != -1" strong="true" color="info"/>
        <field name="name"/>
        <field name="elementSelect"/>
        <field name="typeSelect"/>
        <field name="sequence"/>
    </grid>
    
	<form name="mrp-line-type-form" model="com.axelor.apps.supplychain.db.MrpLineType" title="MRP Type">
	    <panel name="main" >
	        <field name="code"/>
   	        <field name="label"/>
   	        <field name="name"/>
        	<field name="elementSelect"/>
        	<field name="typeSelect"/>
        	<field name="sequence"/>
	    </panel>
	 </form>
	 
</object-views>
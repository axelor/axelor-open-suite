<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">

  <grid name="mrp-line-origin-grid" model="com.axelor.apps.supplychain.db.MrpLineOrigin"
    title="Origins">
    <field name="relatedToSelect"/>
    <field name="relatedToSelectId"/>
  </grid>

  <form name="mrp-line-origin-form" model="com.axelor.apps.supplychain.db.MrpLineOrigin"
    title="Origin">
    <panel name="mainPanel">
      <field name="relatedToSelect" widget="RefSelect" x-related="relatedToSelectId"
        colSpan="12"/>
    </panel>
  </form>

</object-views>
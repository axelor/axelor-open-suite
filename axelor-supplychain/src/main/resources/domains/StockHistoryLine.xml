<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_6.1.xsd">

  <module name="stock" package="com.axelor.apps.stock.db"/>

  <entity name="StockHistoryLine" persistable="false">
    <decimal name="sumOneoffSaleOutQtyPeriod" readonly="true"
      title="One-off sale outgoing quantity" precision="20" scale="10"/>
    <decimal name="sumOutQtyPeriod" readonly="true"
      title="Outgoing quantity (One-off sale excluded)" precision="20" scale="10"/>
  </entity>
</domain-models>

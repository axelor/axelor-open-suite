      check-message="Currency with code :currency_code doesn't exist"/>
    <bind column="statusSelect" to="statusSelect"
      check="com.axelor.apps.base.service.imports.importer.ImportHelper.checkSelection('purchase.purchase.order.status.select', statusSelect, false)"
      check-message="Invalid statusSelect :statusSelect"/>
    <bind column="receiptState" to="receiptState"
      check="com.axelor.apps.base.service.imports.importer.ImportHelper.checkSelection('purchase.order.receipt.state', receiptState, false)"

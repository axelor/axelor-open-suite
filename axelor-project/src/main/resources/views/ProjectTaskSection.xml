<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">

  <grid name="project-task-section-grid" title="Section"
    model="com.axelor.apps.project.db.ProjectTaskSection" orderBy="sequence">
    <field name="name"/>
    <field name="sequence"/>
  </grid>

  <form name="project-task-section-form" title="Section"
    model="com.axelor.apps.project.db.ProjectTaskSection">
    <panel name="mainPanel">
      <field name="name"/>
      <field name="sequence"/>
    </panel>
  </form>

</object-views>
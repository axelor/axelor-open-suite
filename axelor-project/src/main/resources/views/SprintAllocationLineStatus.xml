<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.1.xsd">

  <grid name="sprint-allocation-line-status-grid" title="Sprint allocation line status"
    model="com.axelor.apps.project.db.SprintAllocationLineStatus" orderBy="sequence" canMove="true">

    <field name="name"/>
    <field name="sequence" hidden="true"/>

  </grid>

  <form name="sprint-allocation-line-status-form" title="Sprint allocation line status"
    model="com.axelor.apps.project.db.SprintAllocationLineStatus">

    <panel name="mainPanel">
      <field name="name"/>
      <field name="sequence" hidden="true"/>
    </panel>

  </form>

</object-views>

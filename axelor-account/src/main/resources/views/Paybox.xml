<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.1.xsd">

	<grid name="paybox-grid" title="Paybox" model="com.axelor.apps.account.db.Paybox">
		<field name="name"/>
	</grid>

	<form name="paybox-form" title="Paybox" model="com.axelor.apps.account.db.Paybox" width="large">
		<panel name="mainPanel">
			<field name="name" hidden="true"/>
			<label name="refreshLabel" title="Please close this notebook to go back to the payment voucher. Please refresh the payment voucher to see the changes."/>
		</panel>
	</form>

</object-views>
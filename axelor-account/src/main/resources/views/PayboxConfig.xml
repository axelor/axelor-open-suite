<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.1.xsd">
    
    <grid name="paybox-config-grid" title="Paybox configurations" model="com.axelor.apps.account.db.PayboxConfig">
        <field name="name"/>
        <field name="code" x-bind="{{code|unaccent|uppercase}}"/>
    </grid>
    
    <form name="paybox-config-form" title="Paybox configuration" model="com.axelor.apps.account.db.PayboxConfig"> 
        <panel name="mainPanel" >
	        <field name="name"/>
	       	<field name="code" x-bind="{{code|unaccent|uppercase}}"/>
	       	<field name="payboxUrl"  />
	       	<field name="payboxSite"/>
	       	<field name="payboxRang"/>
	       	<field name="payboxDevise"/>
	       	<field name="payboxIdentifiant"/>
	       	<field name="payboxHashSelect"/>
	       	<field name="payboxDefaultEmail"/>
			<field name="payboxRetour"  />
			<field name="payboxHmac"  />
	       	<field name="payboxRetourUrlEffectue"  />
	       	<field name="payboxRetourUrlRefuse"  />
	       	<field name="payboxRetourUrlAnnule"  />
			<field name="payboxPublicKeyPath"  />
        </panel>
        
    </form>
	
	
</object-views>
    <view type="grid" name="payment-voucher-grid"/>
    <view type="form" name="payment-voucher-form"/>
    <view-param name="search-filters" value="payment-voucher-filters"/>
    <context name="_operationTypeSelect" expr="eval: 3"/>
  </action-view>

  <menuitem name="account-root-payments-suppl-invoices-to-pay"
    parent="account-root-payments" title="Suppl. Invoices to pay"
    action="account.root.payments.suppl.invoices.to.pay"/>

  <action-view name="account.root.payments.suppl.invoices.to.pay"
    title="Suppl. Invoices to pay" model="com.axelor.apps.account.db.Invoice">
    <view type="grid" name="invoice-supplier-grid"/>
    <view type="form" name="invoice-form"/>
    <view-param name="search-filters" value="supplier-invoices-filters"/>
    <domain>self.operationTypeSelect = 1 and self.statusSelect = 3 and self.amountRemaining != 0 and
      self.pfpValidateStatusSelect in :_pfpStatus</domain>
    <context name="_operationTypeSelect" expr="eval:1"/>
    <context name="todayDate"
      expr="eval:__config__.app.getTodayDate(__user__.activeCompany)"/>
    <context name="_pfpStatus"
      expr="eval:__config__.app.getApp('account').getActivatePassedForPayment()?[2]:[1,2,3]"/>
  </action-view>


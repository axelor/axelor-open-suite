    <view type="form" name="move-line-form"/>
    <view-param name="search-filters" value="move-line-filters"/>
  </action-view>

  <menuitem name="account-root-payments" parent="account-root" title="Payments" order="5"/>

  <menuitem name="account-root-payments-payment-voucher" parent="account-root-payments"
    title="Payments assistant" action="account.root.payments.payment.voucher"/>

  <action-view name="account.root.payments.payment.voucher" title="Payments assistant"

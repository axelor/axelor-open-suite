<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

  <menuitem name="invoice-root" title="Invoicing" order="-100"
    if="__config__.app.isApp('invoice')" icon="fa-file-text-o" icon-background="#31b4a9"/>

  <menuitem name="invoice-root-custinvoices" parent="invoice-root" order="5"
    title="Cust. Invoices" action="invoice.root.custinvoices"/>

  <action-view name="invoice.root.custinvoices" title="Cust. Invoices"
    model="com.axelor.apps.account.db.Invoice">
    <view type="grid" name="invoice-grid"/>
    <view type="form" name="invoice-form"/>
    <view-param name="search-filters" value="customer-invoices-filters"/>
    <domain>self.operationTypeSelect = 3</domain>
    <context name="_operationTypeSelect" expr="eval:3"/>
    <context name="todayDate"
      expr="eval:__config__.app.getTodayDate(__user__.activeCompany)"/>
  </action-view>

  <menuitem name="invoice-root-custrefunds" parent="invoice-root" order="10"
    title="Cust. Refunds" action="invoice.root.custrefunds"/>

  <action-view name="invoice.root.custrefunds" title="Cust. Refunds"
    model="com.axelor.apps.account.db.Invoice">
    <view type="grid" name="invoice-refund-grid"/>
    <view type="form" name="invoice-form"/>
    <view-param name="search-filters" value="customer-refunds-filters"/>
    <domain>self.operationTypeSelect = 4</domain>
    <context name="_operationTypeSelect" expr="eval:4"/>
    <context name="todayDate"
      expr="eval:__config__.app.getTodayDate(__user__.activeCompany)"/>
  </action-view>

  <menuitem name="invoice-root-purchase-supinvoices" parent="invoice-root" order="15"
    title="Suppl. Invoices" action="invoice.root.purchase.supinvoices"/>

  <action-view name="invoice.root.purchase.supinvoices" title="Suppl. Invoices"
    model="com.axelor.apps.account.db.Invoice">
    <view type="grid" name="invoice-supplier-grid"/>
    <view type="form" name="invoice-form"/>
    <view-param name="search-filters" value="supplier-invoices-filters"/>
    <domain>self.operationTypeSelect = 1</domain>
    <context name="_operationTypeSelect" expr="eval:1"/>
    <context name="todayDate"
      expr="eval:__config__.app.getTodayDate(__user__.activeCompany)"/>
  </action-view>

  <menuitem name="invoice-root-my-awaiting-pfp" title="My awaiting PFP"
    action="invoice.root.my.awaiting.pfp" parent="invoice-root"
    if="__config__.app.getApp('account').getActivatePassedForPayment() &amp;&amp; !__user__.isSuperPfpUser"
    order="16"/>

  <menuitem name="invoice-root-pfp" title="PFP" parent="invoice-root"
    if="__config__.app.getApp('account').getActivatePassedForPayment() &amp;&amp; __user__.isSuperPfpUser"
    order="16"/>

  <menuitem name="invoice-root-pfp-my-awaiting-pfp" title="My awaiting PFP"
    action="invoice.root.my.awaiting.pfp" parent="invoice-root-pfp"
    if="__config__.app.getApp('account').getActivatePassedForPayment()"/>

  <action-view name="invoice.root.my.awaiting.pfp" title="My awaiting PFP"
    model="com.axelor.apps.account.db.Invoice">
    <view type="grid" name="invoice-supplier-grid"/>
    <view type="form" name="invoice-form"/>
    <view-param name="search-filters" value="supplier-invoices-filters"/>
    <domain>self.statusSelect != 4 and self.pfpValidateStatusSelect in (1,3) and
      self.pfpValidatorUser = :_currentUser</domain>
    <context name="_operationTypeSelect" expr="eval:1"/>
    <context name="_currentUser" expr="eval:__user__"/>
  </action-view>

  <menuitem name="invoice-root-pfp-awaiting-pfp" title="awaitingPfpMenuTitle"
    action="invoice.root.pfp.awaiting.pfp" parent="invoice-root-pfp"
    if="__config__.app.getApp('account').getActivatePassedForPayment() &amp;&amp; __user__.isSuperPfpUser"/>

  <action-view name="invoice.root.pfp.awaiting.pfp" title="awaitingPfpMenuTitle"
    model="com.axelor.apps.account.db.Invoice">
    <view type="grid" name="invoice-supplier-grid"/>
    <view type="form" name="invoice-form"/>
    <view-param name="search-filters" value="supplier-invoices-filters"/>
    <domain>self.statusSelect != 4 and self.pfpValidateStatusSelect in (1,3)</domain>
    <context name="_operationTypeSelect" expr="eval:1"/>
  </action-view>

  <menuitem name="invoice-root-purchase-suprefunds" parent="invoice-root" order="20"
    title="Suppl. Refunds" action="invoice.root.purchase.suprefunds"/>

  <action-view name="invoice.root.purchase.suprefunds" title="Suppl. Refunds"
    model="com.axelor.apps.account.db.Invoice">
    <view type="grid" name="invoice-supplier-refund-grid"/>
    <view type="form" name="invoice-form"/>
    <view-param name="search-filters" value="supplier-refunds-filters"/>
    <domain>self.operationTypeSelect = 2</domain>
    <context name="_operationTypeSelect" expr="eval:2"/>
    <context name="todayDate"
      expr="eval:__config__.app.getTodayDate(__user__.activeCompany)"/>
  </action-view>



<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">

  <grid name="analytic-grouping-value-grid" title="Analytic Grouping Values"
    model="com.axelor.apps.account.db.AnalyticGroupingValue">
    <field name="code"/>
    <field name="name"/>
    <field name="analyticGrouping.analyticAxis" title="Analytic Axis related"/>
    <field name="analyticGrouping"/>
  </grid>

  <grid name="analytic-grouping-value-grouping-grid" title="Analytic Grouping Values"
    model="com.axelor.apps.account.db.AnalyticGroupingValue">
    <field name="code"/>
    <field name="name"/>
  </grid>

  <form name="analytic-grouping-value-form" title="Analytic Grouping Value"
    model="com.axelor.apps.account.db.AnalyticGroupingValue">
    <panel name="mainPanel">
      <field name="code"/>
      <field name="name"/>
      <field name="analyticGrouping" required="true"/>
    </panel>
  </form>

  <form name="analytic-grouping-value-grouping-form" title="Analytic Grouping Value"
    model="com.axelor.apps.account.db.AnalyticGroupingValue">
    <panel name="mainPanel">
      <field name="code"/>
      <field name="name"/>
    </panel>
  </form>

</object-views>

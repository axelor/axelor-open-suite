<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">

  <grid name="move-template-type-grid" title="Move template type"
    model="com.axelor.apps.account.db.MoveTemplateType">
    <field name="name"/>
    <field name="code" x-bind="{{code|unaccent|uppercase}}"/>
    <field name="typeSelect"/>
  </grid>

  <form name="move-template-type-form" title="Move template type"
    model="com.axelor.apps.account.db.MoveTemplateType">
    <panel name="mainPanel">
      <field name="name"/>
      <field name="code" x-bind="{{code|unaccent|uppercase}}"/>
      <field name="typeSelect" required="true"/>
    </panel>

  </form>


</object-views>
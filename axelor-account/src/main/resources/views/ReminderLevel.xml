	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_4.0.xsd">
    
    <grid name="reminder-level-grid" title="Reminder levels" model="com.axelor.apps.account.db.ReminderLevel">
        <field name="name"/>

<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.2.xsd">
 
  <module name="account" package="com.axelor.apps.account.db"/>
 
  <entity name="DebtRecoveryMethod" lang="java" cacheable="true">
  
    <string name="name" title="Method name" />
    <string name="code" title="Code" unique="true"/>
    
  	<one-to-many name="debtRecoveryMethodLineList" ref="com.axelor.apps.account.db.DebtRecoveryMethodLine" mappedBy="debtRecoveryMethod" title="Debt recovery method lines"/>
    
  </entity>
  
</domain-models>

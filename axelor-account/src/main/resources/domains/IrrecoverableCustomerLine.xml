<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.3.xsd">

  <module name="account" package="com.axelor.apps.account.db"/>

  <entity name="IrrecoverableCustomerLine" lang="java">
  
  	<many-to-one name="partner" ref="com.axelor.apps.base.db.Partner" title="Customer"/>
  	<many-to-many name="paymentScheduleLineSet" ref="com.axelor.apps.account.db.PaymentScheduleLine" title="Payment(s) schedule(s) rejected"/>
  	<one-to-many name="irrecoverableInvoiceLineList" ref="com.axelor.apps.account.db.IrrecoverableInvoiceLine" mappedBy="irrecoverableCustomerLine" title="Invoices"/>
  	<one-to-many name="irrecoverablePaymentScheduleLineLineList" ref="com.axelor.apps.account.db.IrrecoverablePaymentScheduleLineLine" mappedBy="irrecoverableCustomerLine" title="Invoices"/>
    <many-to-one name="irrecoverable" ref="com.axelor.apps.account.db.Irrecoverable"/>
 
  </entity>

</domain-models>

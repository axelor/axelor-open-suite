<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.1.xsd">

  <module name="account" package="com.axelor.apps.account.db"/>

  <entity name="IrrecoverablePaymentScheduleLineLine" lang="java">
  
  	<many-to-one name="irrecoverableCustomerLine" ref="com.axelor.apps.account.db.IrrecoverableCustomerLine" title="Customer" index="idx_acc_irreco_paym_schedule_line_line_irreco_cust_line"/>

  	<many-to-one name="paymentScheduleLine" ref="com.axelor.apps.account.db.PaymentScheduleLine" title="Rejected schedule line" index="idx_acc_irreco_paym_schedule_line_line_paym_schedule_line"/>
  	<one-to-many name="irrecoverableReportLineList" ref="com.axelor.apps.account.db.IrrecoverableReportLine" mappedBy="irrecoverablePaymentScheduleLineLine" title="Reportings"/>
 	
 	<string name="name" title="Name">
 		<![CDATA[
	    	if (paymentScheduleLine != null) return paymentScheduleLine.getName();
	    	else return "";
	    ]]>
	</string>

  </entity>

</domain-models>

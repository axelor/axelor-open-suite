      <field name="purchFinancialDiscountAccount" on="UPDATE"/>
      <field name="saleFinancialDiscountAccount" on="UPDATE"/>
      <field name="purchFinancialDiscountTax" on="UPDATE"/>
      <field name="saleFinancialDiscountTax" on="UPDATE"/>

      <field name="holdBackCustomerAccount" on="UPDATE"/>
      <field name="holdBackSupplierAccount" on="UPDATE"/>

      <field name="temporaryClosureAuthorizedRoleList" on="UPDATE"/>
      <field name="closureAuthorizedRoleList" on="UPDATE"/>
      <field name="moveOnTempClosureAuthorizedRoleList" on="UPDATE"/>

      <field name="prepaidExpensesAccount" on="UPDATE"/>
      <field name="deferredIncomesAccount" on="UPDATE"/>

      <field name="resultProfitAccount" on="UPDATE"/>
      <field name="resultLossAccount" on="UPDATE"/>


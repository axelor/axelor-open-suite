      <field name="purchaseJournalType" on="UPDATE"/>
      <field name="cashJournalType" on="UPDATE"/>
      <field name="creditNoteJournalType" on="UPDATE"/>

      <field name="supplierAdvancePaymentAccount" on="UPDATE"/>

      <field name="customerAccount" on="UPDATE"/>
      <field name="supplierAccount" on="UPDATE"/>
      <field name="doubtfulCustomerAccount" on="UPDATE"/>
      <field name="employeeAccount" on="UPDATE"/>
      <field name="irrecoverableAccount" on="UPDATE"/>
      <field name="cashPositionVariationAccount" on="UPDATE"/>
      <field name="advancePaymentAccount" on="UPDATE"/>

      <field name="factorCreditAccount" on="UPDATE"/>
      <field name="yearOpeningAccount" on="UPDATE"/>
      <field name="yearClosureAccount" on="UPDATE"/>
      <field name="billOfExchReceivAccount" on="UPDATE"/>
      <field name="accountCodeNbrCharSelect" on="UPDATE"/>

      <field name="partnerAccountGenerationModeSelect" on="UPDATE"/>
      <field name="customerAccountPrefix" on="UPDATE"/>
      <field name="supplierAccountPrefix" on="UPDATE"/>
      <field name="employeeAccountPrefix" on="UPDATE"/>
      <field name="customerAccountSequence" on="UPDATE"/>
      <field name="supplierAccountSequence" on="UPDATE"/>
      <field name="employeeAccountSequence" on="UPDATE"/>

      <field name="thresholdDistanceFromRegulation" on="UPDATE"/>

      <field name="autoReconcileOnInvoice" on="UPDATE"/>
      <field name="autoReconcileOnPayment" on="UPDATE"/>

      <field name="inPaymentMode" on="UPDATE"/>
      <field name="outPaymentMode" on="UPDATE"/>

      <field name="defPaymentCondition" on="UPDATE"/>
      <field name="cfonbConfig" on="UPDATE"/>

      <field name="sixMonthDebtPassReason" on="UPDATE"/>
      <field name="threeMonthDebtPassReason" on="UPDATE"/>
      <field name="sixMonthDebtMonthNumber" on="UPDATE"/>
      <field name="threeMonthDebtMontsNumber" on="UPDATE"/>

      <field name="debtRecoveryConfigLineList" on="UPDATE"/>
      <field name="mailTransitTime" on="UPDATE"/>
      <field name="factorPartner" on="UPDATE"/>

      <field name="reimbursementExportFolderPathCFONB" on="UPDATE"/>
      <field name="reimbursementExportFolderPath" on="UPDATE"/>
      <field name="lowerThresholdReimbursement" on="UPDATE"/>
      <field name="upperThresholdReimbursement" on="UPDATE"/>
      <field name="reimbursementAccount" on="UPDATE"/>
      <field name="reimbursementTemplate" on="UPDATE"/>

      <field name="reimbursementImportFolderPathCFONB" on="UPDATE"/>
      <field name="tempReimbImportFolderPathCFONB" on="UPDATE"/>

      <field name="exportPath" on="UPDATE"/>
      <field name="exportFileName" on="UPDATE"/>

      <field name="paymentScheduleExportFolderPathCFONB" on="UPDATE"/>

      <field name="directDebitPaymentMode" on="UPDATE"/>
      <field name="rejectionPaymentMode" on="UPDATE"/>
      <field name="paymentScheduleRejectNumLimit" on="UPDATE"/>
      <field name="invoiceRejectNumLimit" on="UPDATE"/>
      <field name="rejectPaymentScheduleTemplate" on="UPDATE"/>

      <field name="clearanceAccountSet" on="UPDATE"/>
      <field name="profitAccount" on="UPDATE"/>
      <field name="standardRateTax" on="UPDATE"/>

      <field name="irrecoverableReasonPassage" on="UPDATE"/>
      <field name="irrecoverableStandardRateTax" on="UPDATE"/>


      <field name="generateMoveForInvoicePayment" on="UPDATE"/>
      <field name="generateMoveForAdvancePayment" on="UPDATE"/>
      <field name="fixedAssetCatReqOnInvoice" on="UPDATE"/>
      <field name="isPrintInvoicesInCompanyLanguage" on="UPDATE"/>

      <field name="accountChart" on="UPDATE"/>
      <field name="hasChartImported" on="UPDATE"/>
      <field name="invoiceInAtiSelect" on="UPDATE"/>

      <field name="accountingDaybook" on="UPDATE"/>

      <field name="custInvSequence" on="UPDATE"/>
      <field name="custRefSequence" on="UPDATE"/>
      <field name="suppInvSequence" on="UPDATE"/>
      <field name="suppRefSequence" on="UPDATE"/>

      <field name="invoicingProduct" on="UPDATE"/>
      <field name="advancePaymentProduct" on="UPDATE"/>

      <field name="displayDelAddrOnPrinting" on="UPDATE"/>
      <field name="displayProductCodeOnPrinting" on="UPDATE"/>
      <field name="displayTaxDetailOnPrinting" on="UPDATE"/>
      <field name="displayPartnerSeqOnPrinting" on="UPDATE"/>
      <field name="displayHeadOfficeAddrOnInvoicePrinting" on="UPDATE"/>
      <field name="displayStockMoveOnInvoicePrinting" on="UPDATE"/>
      <field name="invoiceClientBox" on="UPDATE"/>
      <field name="saleInvoiceLegalNote" on="UPDATE"/>
      <field name="termsAndConditions" on="UPDATE"/>
      <field name="invoiceWatermark" on="UPDATE"/>

      <field name="invoiceAutomaticMail" on="UPDATE"/>
      <field name="invoiceMessageTemplate" on="UPDATE"/>

      <field name="isManagePassedForPayment" on="UPDATE"/>
      <field name="isManagePFPInRefund" on="UPDATE"/>

      <field name="lineMinBeforeLongReportGenerationMessageNumber" on="UPDATE"/>

      <field name="isDescriptionRequired" on="UPDATE"/>
      <field name="isActivateSimulatedMove" on="UPDATE"/>

      <field name="hasLatePaymentAccountBlocking" on="UPDATE"/>
      <field name="numberOfDaysBeforeAccountBlocking" on="UPDATE"/>


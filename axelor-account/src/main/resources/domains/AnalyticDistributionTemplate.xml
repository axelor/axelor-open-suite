<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="account" package="com.axelor.apps.account.db"/>

  <entity name="AnalyticDistributionTemplate" lang="java">

    <string name="name" title="Name" required="true"/>
    <one-to-many name="analyticDistributionLineList"
      ref="com.axelor.apps.account.db.AnalyticDistributionLine" title="Analytic distribution lines"
      mappedBy="analyticDistributionTemplate"/>
    <many-to-one name="company" ref="com.axelor.apps.base.db.Company" title="Company"/>
  </entity>

</domain-models>

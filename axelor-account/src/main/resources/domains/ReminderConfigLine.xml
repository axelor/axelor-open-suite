  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_4.0.xsd">

  <module name="account" package="com.axelor.apps.account.db"/>
  
  <entity name="ReminderConfigLine" lang="java" cachable="true">
  
    <integer name="importId"/>
    <many-to-one name="partnerCategory" ref="com.axelor.apps.base.db.PartnerCategory" title="Partner category"/>  
    <many-to-one name="reminderMethod" ref="com.axelor.apps.account.db.ReminderMethod" title="Reminder method"/>
    <many-to-one name="accountConfig" ref="com.axelor.apps.account.db.AccountConfig" title="Account configuration"/>
  
  </entity>

 
</domain-models>

 
 
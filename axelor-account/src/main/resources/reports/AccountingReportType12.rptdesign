            <xml-property name="queryText"><![CDATA[Select 
MoveLine.name as moveLineName,
MoveLine.date_val as moveLineDate,
Account.code as accountCode,
Account.name as accountName,
Tax.code as taxCode,
AccountType.technical_type_select,
Case  when AccountType.technical_type_select != 'tax' then MoveLine.debit
end as debit,
Case when AccountType.technical_type_select = 'tax' then MoveLine.debit
end as debitTaxAmount,
Case  when AccountType.technical_type_select != 'tax' then MoveLine.credit
end as credit,
Case when AccountType.technical_type_select = 'tax' then MoveLine.credit
end as creditTaxAmount


from public.account_accounting_report as AccountingReport 
left outer join public.account_move_line as MoveLine on (AccountingReport.id = ?)
left outer join public.account_account as Account on (MoveLine.account = Account.id)
left outer join public.account_account_type as AccountType on (Account.account_type = AccountType.id)
left outer join public.account_move as Move on (MoveLine.move = Move.id)
left outer join public.account_tax_line as TaxLine on (MoveLine.tax_line = TaxLine.id) 
left outer join public.account_tax as Tax on (TaxLine.tax = Tax.id) 
where 
(AccountingReport.currency = Move.company_currency) 
AND AccountingReport.company = Move.company 
AND MoveLine.date_val >= AccountingReport.date_from 
AND MoveLine.date_val <= AccountingReport.date_to 
AND (Move.status_select = 3 OR Move.status_select = 2) 
AND MoveLine.tax_line is not null
AND Tax.type_select = 2
AND (AccountingReport.display_closing_accounting_moves is true 
	OR(AccountingReport.display_closing_accounting_moves is false 
		AND (Move.functional_origin_select IS NULL OR Move.functional_origin_select != 2)))
AND (AccountingReport.display_opening_accounting_moves is true 
	OR(AccountingReport.display_opening_accounting_moves is false 
		AND (Move.functional_origin_select IS NULL OR Move.functional_origin_select != 1)))
order by MoveLine.date_val ASC, MoveLine.name ASC]]></xml-property>

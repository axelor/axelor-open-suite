            <xml-property name="queryText"><![CDATA[select DebtRecoveryHistory.reference_date,
coalesce(MetaFile.file_path) as logo_path, 
DebtRecoveryHistory.debt_recovery_date,
AccountDebtRecoveryMethodLine.printing_message,
AccountDebtRecoveryMethodLine.specific_printing_message,
Partner.simple_full_name as PartnerName, 
PartnerAddress.addressl4 as street,
PartnerAddress.addressl6 as postalCodeAndCity,
Country.name as CountryName,
PrintingSettings.pdf_header as header, 
PrintingSettings.pdf_footer as footer,
PrintingSettings.logo_position_select as logoPosition,
Company.height as logo_height,
Company.width as logo_width
from account_debt_recovery_history DebtRecoveryHistory
left join account_debt_recovery AccountDebtRecovery on DebtRecoveryHistory.debt_recovery = AccountDebtRecovery.id
left join account_debt_recovery_method_line AccountDebtRecoveryMethodLine on DebtRecoveryHistory.debt_recovery_method_line =  AccountDebtRecoveryMethodLine.id
Left join base_company Company on AccountDebtRecovery.company = Company.id
Left join base_printing_settings PrintingSettings on Company.printing_settings = PrintingSettings.id
Left join account_accounting_situation AccountingSituation on AccountDebtRecovery.accounting_situation = AccountingSituation.id
Left join base_partner Partner on AccountingSituation.partner = Partner.id
Left join base_address PartnerAddress on Partner.main_address = PartnerAddress.id
Left join base_country Country on PartnerAddress.addressl7country = Country.id
left join meta_file as MetaFile on (MetaFile.id = Company.logo)
where DebtRecoveryHistory.id = ?]]></xml-property>

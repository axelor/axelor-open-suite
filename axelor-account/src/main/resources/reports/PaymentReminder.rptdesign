            <xml-property name="queryText"><![CDATA[(select Currency.code as CurrencyCode,
Invoice.invoice_id as IDCode,
Invoice.due_date as DueDate,
DATE_PART('day', NOW()-Invoice.due_date) as LatePaymentDays,
Invoice.in_tax_total as Amount,
Invoice.amount_paid as AmountPaid,
Invoice.amount_remaining as ToPay
from account_debt_recovery_history DebtRecoveryHistory
left join account_debt_recovery DebtRecovery on DebtRecoveryHistory.debt_recovery = DebtRecovery.id
left join account_debt_recovery_invoice_debt_recovery_set DebtRecoveryInvoiceSet on DebtRecovery.id = DebtRecoveryInvoiceSet.account_debt_recovery
left join account_invoice Invoice  on DebtRecoveryInvoiceSet.invoice_debt_recovery_set = Invoice.id
left join base_currency Currency on Invoice.currency = Currency.id
where DebtRecoveryHistory.id = ?
and Currency.code is not null)
Union
(select Currency.code as CurrencyCode,
PaymentScheduleLine.name as IDCode,
PaymentScheduleLine.schedule_date as DueDate,
DATE_PART('day', NOW()-PaymentScheduleLine.schedule_date) as LatePaymentDays,
PaymentScheduleLine.in_tax_amount as Amount,
PaymentScheduleLine.in_tax_amount_paid as AmountPaid,
PaymentScheduleLine.in_tax_amount - PaymentScheduleLine.in_tax_amount_paid as ToPay
from account_debt_recovery_history DebtRecoveryHistory
left join account_debt_recovery DebtRecovery on DebtRecoveryHistory.debt_recovery = DebtRecovery.id
left join account_debt_recovery_payment_schedule_line_debt_recovery_set DebtRecoveryPaymentScheduleLineSet on DebtRecovery.id = DebtRecoveryPaymentScheduleLineSet.account_debt_recovery
left join account_payment_schedule_line PaymentScheduleLine on DebtRecoveryPaymentScheduleLineSet.payment_schedule_line_debt_recovery_set = PaymentScheduleLine.id
left join account_payment_schedule PaymentSchedule on PaymentScheduleLine.payment_schedule = PaymentSchedule.id
left join base_currency Currency on PaymentSchedule.currency = Currency.id
where DebtRecoveryHistory.id = ?
and Currency.code is not null)]]></xml-property>

            <xml-property name="queryText"><![CDATA[SELECT
	ConfigLine.type_select 				  as type,
	ConfigLine.label 					  as rule_label,
	ConfigLine.code 					  as rule_code,
	ConfigLine.rule_type_select 		  as rule_type,
	ConfigLine.result_select 			  as result,
	ConfigLine.hidden 					  as hidden,
	ConfigLine.rule	 					  as rule,
	SUM(MoveLine.debit - MoveLine.credit) as account_diff
	
FROM account_accounting_report as Report

JOIN account_accounting_report_type as ReportType
ON Report.report_type = ReportType.id

LEFT JOIN account_accounting_report_config_line as ConfigLine
ON ReportType.id = ConfigLine.report_type

LEFT JOIN account_accounting_report_config_line_account_type_set as AccountTypeSet
ON AccountTypeSet.account_accounting_report_config_line = ConfigLine.id

LEFT JOIN account_account_type as AccountType
ON AccountType.id = AccountTypeSet.account_type_set

LEFT JOIN account_accounting_report_config_line_account_set as AccountSet
ON AccountSet.account_accounting_report_config_line = ConfigLine.id

LEFT JOIN account_account as Account
ON Account.id = AccountSet.account_set OR Account.account_type = AccountType.id

LEFT JOIN account_move_line as MoveLine
ON MoveLine.account = Account.id 

LEFT JOIN account_move as Move
ON MoveLine.move = Move.id

LEFT JOIN account_accounting_report_partner_set as PartnerSet
ON PartnerSet.account_accounting_report = Report.id

WHERE Report.id = ?
	AND (Report.date_from    IS NULL OR MoveLine.date_val >= Report.date_from)
	AND (Report.date_to      IS NULL OR MoveLine.date_val <= Report.date_to)
	AND (Report.journal      IS NULL OR Move.journal   	  = Report.journal)
	AND (Report.period	     IS NULL OR Move.period		  = Report.period)
	AND (Report.payment_mode IS NULL OR Move.payment_mode  = Report.payment_mode)
	AND (PartnerSet.account_accounting_report IS NULL OR Move.partner = PartnerSet.partner_set)
	AND (MoveLine.id 	     IS NULL OR (
		Report.currency	  = Move.company_currency
		AND Report.company = Move.company
		AND Move.ignore_in_accounting_ok IN ('false', null) 
		AND Move.status_select IN (2, 3)
	))
	
GROUP BY ConfigLine.id

ORDER BY ConfigLine.sequence]]></xml-property>

            <xml-property name="queryText"><![CDATA[SELECT
	Report.ref as ref,
	Report.publication_date_time as report_date,
	Report.date_from as date_from,
	Report.date_to as date_to,
	ReportType.report_title as report_title,
	Company.name as company_name,
	Company.height as logo_height,
	Company.width as logo_width,
	Currency.code as currency,
	Journal.name as journal,
	PaymentMode.name as payment_mode,
	Period.name as period,
	MetaFile.file_path as logo_path
	
FROM account_accounting_report as Report

JOIN account_accounting_report_type as ReportType
ON Report.report_type = ReportType.id

LEFT JOIN base_company as Company
ON Company.id = Report.Company

LEFT JOIN base_currency as Currency 
ON Report.currency = Currency.id

LEFT JOIN public.account_journal as Journal 
ON Report.journal = Journal.id

LEFT JOIN account_payment_mode as PaymentMode
ON Report.payment_mode = PaymentMode.id

LEFT JOIN base_period as Period
ON Period.id = Report.period

LEFT JOIN meta_file as MetaFile 
ON MetaFile.id = Company.logo

where Report.id = ?]]></xml-property>

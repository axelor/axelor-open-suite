<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

  <form name="product-category-form" id="portal-product-category-form" title="Product category"
    model="com.axelor.apps.base.db.ProductCategory" extension="true">
    <extend target="//panel[@name='mainPanel']/*[last()]">
      <insert position="after">
        <field name="partnerCategorySet" widget="TagSelect"/>
      </insert>
    </extend>
  </form>

</object-views>

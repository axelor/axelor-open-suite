<?xml version="1.0" encoding="UTF-8"?>
<csv-inputs xmlns="http://axelor.com/xml/ns/data-import"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/data-import http://axelor.com/xml/ns/data-import/data-import_7.4.xsd">

  <input file="stock_stockLocationLine.csv" separator=","
    type="com.axelor.apps.stock.db.StockLocationLine" search="self.importId = :importId"
    call="com.axelor.csv.script.ImportStockLocationLine:importStockLocationLine">
    <bind to="stockLocation" column="stockLocation_importId"
      search="self.importId = :stockLocation_importId"
      if="stockLocation_importId != null &amp;&amp; stockLocation_importId != ''"/>
    <bind to="detailsStockLocation" column="detailsStockLocation_importId"
      search="self.importId = :detailsStockLocation_importId"
      if="detailsStockLocation_importId != null &amp;&amp; detailsStockLocation_importId != ''"/>
    <bind to="trackingNumber"
      if="trackingNumber_importId != null &amp;&amp; trackingNumber_importId != ''">
      <bind to="product" search="self.importId = :product_importId"/>
      <bind to="trackingNumberSeq" eval="eval: trackingNumber_importId"/>
    </bind>
    <bind to="product" search="self.importId = :product_importId"/>
    <bind to="unit" search="self.importId = :unit_importId"/>
    <bind to="lastInventoryRealQty" column="lastInventoryRealQty"/>
    <bind to="avgPrice" column="avgPrice"/>
    <bind to="lastInventoryDateT" column="lastInventoryDateT"/>
    <bind column="rack" to="rack"/>
    <bind to="currentQty" column="qty"/>
    <bind to="futureQty" column="qty"/>
  </input>

</csv-inputs>

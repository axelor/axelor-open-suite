<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">

  <grid name="freight-carrier-customer-account-number-grid"
    model="com.axelor.apps.stock.db.FreightCarrierCustomerAccountNumber"
    title="Customer account numbers to carriers">
    <field name="carrierPartner" domain="self.isCarrier = TRUE"/>
    <field name="customerAccountNumber"/>
  </grid>

  <form name="freight-carrier-customer-account-number-form"
    model="com.axelor.apps.stock.db.FreightCarrierCustomerAccountNumber"
    title="Customer account number to carrier">
    <panel name="mainPanel">
      <field name="carrierPartner" domain="self.isCarrier = TRUE"/>
      <field name="customerAccountNumber"/>
    </panel>
  </form>

</object-views>

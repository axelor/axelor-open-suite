<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.2.xsd">

  <grid name="freight-carrier-pricings-grid" title="Freight carrier pricings"
    model="com.axelor.apps.stock.db.FreightCarrierPricing" x-selector="none">
    <field name="freightCarrierMode"/>
    <field name="pricing"/>
  </grid>

  <form name="freight-carrier-pricings-form"
    model="com.axelor.apps.stock.db.FreightCarrierPricing" title="Freight carrier pricing">
    <panel name="mainPanel">
      <field name="freightCarrierMode"/>
      <field name="pricing" form-view="pricing-form" grid-view="pricing-grid"/>
    </panel>
  </form>
</object-views>
<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">
    
    <grid name="partner-product-quality-rating-grid" title="Partner product quality ratings" model="com.axelor.apps.stock.db.PartnerProductQualityRating">
    	<field name="product"/>
    	<field name="qualityRatingSelect" widget="ImageSelect" x-labels="false"/>
    	<field name="arrivalProductQty"/>
    </grid>
    
    <form name="partner-product-quality-rating-form" title="Partner product quality rating" model="com.axelor.apps.stock.db.PartnerProductQualityRating">
    	<panel name="mainPanel">
    		<field name="product" colSpan="6"/>
    		<field name="qualityRatingSelect" widget="ImageSelect" x-labels="false" colSpan="3"/>
    		<field name="arrivalProductQty" colSpan="3"/>
    	</panel>
    </form>
    
 </object-views>
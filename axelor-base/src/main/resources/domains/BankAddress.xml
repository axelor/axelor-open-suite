<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.3.xsd">

    <module name="base" package="com.axelor.apps.base.db"/>

    <entity name="BankAddress" lang="java">
        <string name="code" title="Code"/>
        <string name="address" title="SWIFT address" max="255" nullable="false"/>
        <string name="label" title="Label"/>
        <many-to-one name="bank" ref="com.axelor.apps.base.db.Bank"/>
        <string name="fullAddress" title="Full Address" namecolumn="true"/>
    </entity>
</domain-models>


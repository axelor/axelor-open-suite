<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.3.xsd">

  <module name="base" package="com.axelor.apps.base.db"/>

  <entity name="Routing" lang="java">
  
    <string name="name" title="Name" required="true"/>
    <string name="code" title="Code" required="true"/>
    <integer name="priority" title="Priority"/>
    <boolean name="isActive" title="Active"/>
    
    <one-to-many name="routingRuleList" ref="com.axelor.apps.base.db.RoutingRule" mappedBy="routing" title="Routing rules"/>
    <one-to-many name="routingActionList" ref="com.axelor.apps.base.db.RoutingAction" mappedBy="routing" title="Routing actions"/>
    
    
    
  </entity>

</domain-models>

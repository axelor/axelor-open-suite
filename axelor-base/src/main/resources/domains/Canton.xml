<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.1.xsd">
  
  <module name="base" package="com.axelor.apps.base.db"/>
  
  <entity name="Canton" lang="java" cacheable="true">
  
    <string name="name" title="Name" required="true"/>
    <string name="code" title="Code" unique="true" required="true"/>
    <string name="artmin" title="ARTMIN" selection="iterritory.artmin.select"/>
    <integer name="inhabitantsNbr" title="Nb of Inhabitants"/>
    <many-to-one name="department" title="Department" ref="Department"/>
    
  </entity>
  
</domain-models>

<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_7.1.xsd">

  <module name="base" package="com.axelor.apps.base.db"/>

  <entity name="CommentFile">

    <many-to-one ref="com.axelor.meta.db.MetaFile" name="attachmentFile" title="File"/>
    <string name="description" title="Description" large="true"/>
    <many-to-one ref="com.axelor.apps.base.db.Comment" name="relatedComment"
      title="Comment"/>

  </entity>

</domain-models>

<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.2.xsd">

	<module name="meta" package="com.axelor.meta.db" />

	<entity name="MetaField" table="META_FIELD" cacheable="true" >
    	<string name="label" title="Label" translatable="true"/>
    	
    	<finder-method name="findByModel" using="name, metaModel" cacheable="true"/>
	</entity>
</domain-models>

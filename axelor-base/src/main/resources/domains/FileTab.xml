<?xml version="1.0" ?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.2.xsd">

  <module name="base" package="com.axelor.apps.base.db"/>
	
  <entity name="FileTab">
  	<integer name="sequence" title="Sequence"/>
  	
  	<string name="name" title="Name" required="true"/>
  	
  	<many-to-one name="metaModel" ref="com.axelor.meta.db.MetaModel" title="Object"/>
  	
  	<integer name="importType" title="Import type" selection="file.field.import.type.select" min="1" default="3"/>
  	
  	<many-to-many name="searchFieldSet" ref="com.axelor.apps.base.db.FileField" title="Search fields"/>
  	
  	<one-to-many name="fileFieldList" ref="com.axelor.apps.base.db.FileField" title="Fields configuration" mappedBy="fileTab"/>
    
    <many-to-one name="validationLog" ref="com.axelor.meta.db.MetaFile" title="Validation log"/>
    
    <many-to-one name="advancedImport" ref="com.axelor.apps.base.db.AdvancedImport" title="Advanced import"/>
  </entity>

</domain-models>

<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="base" package="com.axelor.apps.base.db"/>

  <entity name="ResearchPrimaryKey" lang="java">

    <string name="name" title="Name" namecolumn="true"/>
    <string name="code" title="Code" unique="true"/>
    <integer name="typeSelect" title="Type" selection="research.primary.key.type.select"
      default="0"/>
    <one-to-many name="researchParameterList"
      ref="com.axelor.apps.base.db.ResearchParameter" mappedBy="researchPrimaryKey"/>

  </entity>


</domain-models>
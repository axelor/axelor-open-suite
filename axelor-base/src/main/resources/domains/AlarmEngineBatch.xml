<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_6.1.xsd">

  <module name="base" package="com.axelor.apps.base.db"/>

  <entity name="AlarmEngineBatch">

    <!-- HEADER -->
    <string name="code" title="Code" required="true" unique="true"/>
    <many-to-many name="metaModelSet" ref="com.axelor.meta.db.MetaModel" title="Model"
      required="true"/>
    <many-to-many name="alarmEngineSet" ref="com.axelor.apps.base.db.AlarmEngine"
      title="Alarm Engine"/>
    <string name="description" title="Description" large="true"/>
    <one-to-many name="batchList" ref="com.axelor.apps.base.db.Batch"
      mappedBy="alarmEngineBatch" title="Batchs"/>

  </entity>

</domain-models>
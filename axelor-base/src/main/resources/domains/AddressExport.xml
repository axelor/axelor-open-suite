<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.3.xsd">

	<module name="base" package="com.axelor.apps.base.db"/>

	<entity name="AddressExport" lang="java">
	
		<datetime name="date" column="date_val" title="Export Date" tz="true" />
		<string name="path" title="Path to export file" default="/tmp/adresses.csv" namecolumn="true" />
		<string name="log" multiline="true" title="Log" />
		
	</entity>

</domain-models>

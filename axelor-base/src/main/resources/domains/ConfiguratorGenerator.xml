<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_7.1.xsd">

  <module name="base" package="com.axelor.apps.base.db"/>

  <entity name="ConfiguratorGenerator">
    <many-to-one name="metaModel" ref="com.axelor.meta.db.MetaModel"/>
    <many-to-one name="configuratorFormConfig"
      ref="com.axelor.apps.base.db.ConfiguratorFormConfig" title="Configurator form config"/>
    <string name="recordReference"/>
    <integer name="recordReferenceId"/>
    <one-to-many ref="com.axelor.apps.base.db.ConfiguratorGeneratorLine"
      name="configuratorGeneratorLineList" title="Configurator generator lines"
      mappedBy="parentConfiguratorGenerator"/>
  </entity>

</domain-models>

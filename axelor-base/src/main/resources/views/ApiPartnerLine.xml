<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.2.xsd">

  <grid name="api-partner-line-grid" title="API Partner Line"
    model="com.axelor.apps.base.db.ApiPartnerLine">
    <field name="partnerMetaField"/>
    <field name="currentValue"/>
    <field name="apiNameField"/>
    <field name="apiFieldValue"/>
    <field name="toUpdate"/>
  </grid>

  <form name="api-partner-line-form" title="API Partner Line"
    model="com.axelor.apps.base.db.ApiPartnerLine" canDelete="true" canNew="true" width="large">
    <panel>
      <field name="partnerMetaField"/>
      <field name="currentValue"/>
      <field name="apiNameField"/>
      <field name="apiFieldValue"/>
      <field name="toUpdate"/>
    </panel>
  </form>

</object-views>
<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.2.xsd">

  <form name="api-partner-form" title="API Partner" model="com.axelor.apps.base.db.ApiPartner"
    canDelete="false" canNew="false" onNew="action-api-partner-method-init-partner-line-list" width="large">
    <panel colSpan="12">
      <panel-related field="apiPartnerLineList" colSpan="12"
        grid-view="api-partner-line-grid" form-view="api-partner-line-form" canNew="true"
        canRemove="true"/>
    </panel>
  </form>

  <action-method name="action-api-partner-method-init-partner-line-list">
    <call class="com.axelor.apps.base.web.ApiPartnerController" method="initPartnerLineList"/>
  </action-method>

</object-views>
<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.1.xsd">
	
	<form model="com.axelor.apps.base.db.Wizard" title="Convert demo file" name="convert-demo-file-wizard-form">
		<panel name="mainPanel">
			<field name="$importFile" title="File" type="many-to-one"  widget="binary-link" target="com.axelor.meta.db.MetaFile"/>
			<field name="$csvMetaFile" title="Csv generated" type="many-to-one"  widget="binary-link" target="com.axelor.meta.db.MetaFile" readonly="true"/>
			<button name="convertBtn" title="Convert" onClick="action-method-convert-demo-data-file" readonlyIf="$importFile == null"/>
		</panel>
	</form>
	
	<action-method name="action-method-convert-demo-data-file">
		<call class="com.axelor.apps.base.web.ConvertDemoDataFileController" method="convertDemoDataFile"/>
	</action-method>

</object-views>

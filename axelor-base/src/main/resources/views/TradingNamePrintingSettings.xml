    xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">

    <grid model="com.axelor.apps.base.db.TradingNamePrintingSettings" title="List of printing settings for trading names" name="trading-name-printing-settings-company-grid">
        <field name="tradingName" />

<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">

  <grid model="com.axelor.apps.base.db.TradingNamePrintingSettings"
    title="List of printing settings for trading names"
    name="trading-name-printing-settings-company-grid">
    <field name="tradingName"/>
    <field name="printingSettings"/>
  </grid>

  <form model="com.axelor.apps.base.db.TradingNamePrintingSettings"
    title="Printing settings for trading name" name="trading-name-printing-settings-company-form">
    <panel name="mainPanel">
      <field name="tradingName"/>
      <field name="printingSettings"/>
    </panel>
  </form>

</object-views>

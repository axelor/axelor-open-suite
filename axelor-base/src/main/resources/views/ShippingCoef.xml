<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">

	<grid name="shipping-coef-grid" title="Shipping Coef. Table" model="com.axelor.apps.base.db.ShippingCoef">
		<field name="company" if="__config__.app.getApp('base').getEnableMultiCompany()"/>
		<field name="shippingCoef"/>
	</grid>
    
    <form name="shipping-coef-form" title="Shipping Coef. Table" model="com.axelor.apps.base.db.ShippingCoef">
        <panel>
            <field name="company"/>
            <field name="shippingCoef"/>
        </panel>
    </form>

</object-views>
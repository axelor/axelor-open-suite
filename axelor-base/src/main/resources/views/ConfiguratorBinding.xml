<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.2.xsd">

  <grid name="configurator-binding-grid" title="Configurator binding"
    model="com.axelor.apps.base.db.ConfiguratorBinding">
    <field name="metaModel"/>
  </grid>

  <form name="configurator-binding-form" title="Configurator binding"
    model="com.axelor.apps.base.db.ConfiguratorBinding" width="large">
    <panel>
      <field name="metaModel"/>
      <field name="businessLogicLinkScript" form-view="link-script-form"
        grid-view="link-script-grid"/>
    </panel>
  </form>

</object-views>

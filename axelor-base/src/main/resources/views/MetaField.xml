<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.3.xsd">
	
	<grid model="com.axelor.meta.db.MetaField" title="Fields" name="meta-field-light-grid">
		<field name="name"/>
		<field name="typeName"/>
		<field name="label"/>
	</grid>

	<grid name="meta-field-details-grid" title="METAMODEL.DETAILS" model="com.axelor.meta.db.MetaField" groupBy="metaModel">
		<field name="metaModel"/>
		<field name="name"/>
		<field name="typeName"/>
		<field name="label"/>
		<field name="relationship"/>
		<field name="mappedBy"/>
		<field name="metaModel.tableName"/>
	</grid>

</object-views>
<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">
    
    <grid name="partner-category-grid" title="Partner categories" model="com.axelor.apps.base.db.PartnerCategory">
		<field name="name"/>
        <field name="code" x-bind="{{code|unaccent|uppercase}}"/>
    </grid>
    
    <form name="partner-category-form" title="Partner category" model="com.axelor.apps.base.db.PartnerCategory"  >
    	<panel name="mainPanel" >
			<field name="code" x-bind="{{code|unaccent|uppercase}}" />
	        <field name="name"/>
        </panel>
    </form>
    
</object-views>
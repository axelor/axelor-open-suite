<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.4.xsd">

  <grid name="partner-link-type-grid" title="Partner relation type"
    model="com.axelor.apps.base.db.PartnerLinkType" editable="true">
    <field name="name"/>
    <field name="typeSelect"/>
    <field name="link1Name"/>
    <field name="link2Name"/>
  </grid>

  <form name="partner-link-type-form" title="Partner relation type"
    model="com.axelor.apps.base.db.PartnerLinkType">
    <panel name="mainPanel">
      <field name="name"/>
      <field name="typeSelect"/>
      <field name="link1Name"/>
      <field name="link2Name"/>
    </panel>
  </form>

</object-views>

<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_3.0.xsd">
	
	<grid name="import-history-grid" title="Historical" model="com.axelor.apps.base.db.ImportHistory" orderBy="-createdOn">
		<field name="user"/>
		<field name="dataMetaFile"/>
        <field name="createdOn"/>
	</grid>
	
	<form name="import-history-form" cols="6" title="Historical" model="com.axelor.apps.base.db.ImportHistory">
		
		<panel>
			<field name="user" />
			<field name="dataMetaFile" />
	        <field name="createdOn"/>
	        <field name="log" colSpan="12" showTitle="false" />
		</panel>
        
	</form>

</object-views>

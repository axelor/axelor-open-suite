<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">

  <grid name="address-template-grid" title="Address Template"
    model="com.axelor.apps.base.db.AddressTemplate">
    <field name="name"/>
    <field name="engineSelect"/>
  </grid>

  <form name="address-template-form" title="Address Template"
    model="com.axelor.apps.base.db.AddressTemplate">
    <panel name="mainPanel" title="Address Template">
      <field name="name" required="true"/>
      <field name="engineSelect" required="true"/>
      <field name="templateStr" required="true"/>
    </panel>
  </form>


</object-views>
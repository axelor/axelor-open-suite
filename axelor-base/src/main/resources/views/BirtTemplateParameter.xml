  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">

  <grid name="birt-template-parameter-grid" title="Birt Template parameters"
    model="com.axelor.apps.base.db.BirtTemplateParameter">
    <field name="name"/>
    <field name="value"/>
  </grid>

  <form name="birt-template-parameter-form" title="Birt Template parameter"

<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.1.xsd">

  <grid name="tax-type-grid" title="Tax types" model="com.axelor.apps.account.db.TaxType">
    <field name="name"/>
    <field name="code"/>
    <field name="typeSelect"/>
  </grid>

  <form name="tax-type-form" title="Tax type" model="com.axelor.apps.account.db.TaxType">
    <panel name="mainPanel">
      <field name="name"/>
      <field name="code"/>
      <field name="typeSelect"/>
    </panel>
  </form>

</object-views>
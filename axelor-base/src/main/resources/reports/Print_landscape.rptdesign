            <xml-property name="queryText"><![CDATA[SELECT
  PrintingSettings.color_code           AS "colorHex",
  PrintingSettings.logo_position_select AS "logoPosition",
  PrintingSettings.pdf_header           AS "header",
  PrintingSettings.pdf_footer           AS "footer",
  MetaFile.file_path                    AS "logoPath",
  Print.hide_print_settings             AS "hidePrintSettings"
FROM base_printing_settings AS PrintingSettings
  LEFT JOIN base_print AS Print ON (Print.print_settings = PrintingSettings.id)
  LEFT JOIN base_company AS Company ON (Company.id = Print.company)
  LEFT JOIN meta_file as MetaFile on (MetaFile.id = Company.logo)
WHERE Print.id = ?;]]></xml-property>

            <xml-property name="queryText"><![CDATA[SELECT 
    MetaFile.file_path AS logo_path,
	baseCompany.height AS logo_height,
	baseCompany.width AS logo_width,	
    self.id AS message_id,
    basePrintingSettings.pdf_header AS pdf_header,
    metaSelectItem.title AS media_type_select,
    self.subject AS subject,
    self.content AS content,
    basePrintingSettings.pdf_footer,
    self.address_block AS address_block   
FROM 
  message_message self
left outer JOIN base_company AS baseCompany ON baseCompany.id=self.company
left outer join meta_file as MetaFile on (MetaFile.id = baseCompany.logo)
left outer JOIN base_printing_settings AS basePrintingSettings ON basePrintingSettings.id=baseCompany.id
LEFT outer JOIN meta_select AS metaSelect ON metaSelect.name='message.media.type.select'
LEFT outer JOIN meta_select_item AS metaSelectItem ON metaSelectItem.select_id=metaSelect.id AND metaSelectItem.value= CAST(self.media_type_select AS CHAR)
WHERE self.id IN (@INJECTID)]]></xml-property>

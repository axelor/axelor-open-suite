            <xml-property name="queryText"><![CDATA[select Product.id, MetaFile.file_path as logo_path, Company.height as logo_height, Company.width as logo_width,
Product.name as product_name, Product.code as product_code,
ProductPicture.file_path as product_picture,
BarCodeFile.file_path as bar_code_picture, Product.description as product_description, Product.sale_price,
Currency.symbol as currency_symbol, Product.has_warranty, Product.warranty_nbr_of_months,
Product.bar_code as bar_code
from base_product as Product
left join base_company as Company on (Company.id = ?)
left join meta_file as MetaFile on (MetaFile.id = Company.logo)
left join meta_file as ProductPicture on ProductPicture.id = Product.picture
left join meta_file as BarCodeFile on (BarCodeFile.id = Product.bar_code)
left join base_currency as Currency on (Product.sale_currency = Currency.id)
where Product.id = ?
]]></xml-property>

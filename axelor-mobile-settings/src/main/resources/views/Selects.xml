  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.2.xsd">

  <!-- Nomenclature : * name : "interfaceName" + "fieldName" + ".select" -->

  <selection name="mobilesettings.mobile.config.sequence.select">
    <option value="app-stock">Stock</option>
    <option value="app-manufacturing">Manufacturing</option>
    <option value="app-crm">CRM</option>
    <option value="app-helpdesk">Helpdesk</option>
    <option value="app-hr">HR</option>
    <option value="app-quality">Quality</option>
    <option value="app-intervention">Intervention</option>
    <option value="app-sale">Sale</option>
    <option value="app-project">Project</option>
    <option value="app-dms">DMS</option>

<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">
	
	<grid name="docusign-account-grid" title="DocuSign accounts" model="com.axelor.apps.docusign.db.DocuSignAccount">
       <field name="name"/>	   	   
	</grid>
	
	<form name="docusign-account-form" title="DocuSign account" model="com.axelor.apps.docusign.db.DocuSignAccount">
       <panel name="mainPanel" colSpan="12">         
         <field name="name"/>
         <field name="accountId"/>
	     <field name="accessToken" colSpan="12"/>
	     <field name="basePath"/>                      
       </panel>
    </form>
	
</object-views>
<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.3.xsd">

  <module name="docusign" package="com.axelor.apps.docusign.db"/>

  <entity name="DocuSignDocumentSetting" lang="java">

    <string name="name" title="Name"/>
    <string name="documentId" title="Document id"/>
    <many-to-one name="docuSignEnvelopeSetting" title="DocuSign envelope setting"
      ref="com.axelor.apps.docusign.db.DocuSignEnvelopeSetting"/>
    <one-to-many name="docuSignFieldSettingList" title="Field settings"
      ref="com.axelor.apps.docusign.db.DocuSignFieldSetting" mappedBy="docuSignDocumentSetting"/>
    <integer name="sequence" title="Seq."/>
    <string name="fileExtension" title="File extension"/>
    <string name="unsignedMetaFileDefaultPath" title="File default path" large="true"/>

  </entity>

</domain-models>
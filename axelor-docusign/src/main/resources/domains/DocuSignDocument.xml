<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.2.xsd">
  
  <module name="docusign" package="com.axelor.apps.docusign.db"/>

  <entity name="DocuSignDocument" lang="java">

    <string name="name" title="Name" namecolumn="true"/>
    <string name="documentId" title="Document id"/>
    <many-to-one name="docuSignEnvelope" title="DocuSign envelope" ref="com.axelor.apps.docusign.db.DocuSignEnvelope"/>       
    <many-to-one name="unsignedMetaFile" title="Unsigned file" ref="com.axelor.meta.db.MetaFile"/>
    <many-to-one name="signedMetaFile" title="Signed file" ref="com.axelor.meta.db.MetaFile"/>
    <one-to-many name="docuSignFieldList" title="Fields" ref="com.axelor.apps.docusign.db.DocuSignField" mappedBy="docuSignDocument"/>
    <integer name="sequence" title="Seq."/>
    <string name="fileExtension" title="File extension"/>
  
  </entity>
  
  
</domain-models>
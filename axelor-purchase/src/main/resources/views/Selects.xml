<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">


	<!-- Nomenclature : * name : "interfaceName" + "fieldName" + ".select" -->
	
	<selection name="purchase.purchase.order.status.select">
		<option value="1">Draft</option>
		<option value="2">Requested</option>
		<option value="3">Validated</option>
		<option value="4">Finished</option>
		<option value="5">Canceled</option>
	</selection>
	
	<selection name="purchase.order.receipt.state">
		<option value="1">Not received</option>
		<option value="2">Partially received</option>
		<option value="3">Received</option>
	</selection>
	
	
	<selection name="purchase.purchase.order.invoicing.type.select">
		<option value="1">Free</option>
		<option value="2">By delivery</option>
		<option value="3">Per order</option>
	</selection>
	
	<selection name="app.type.select" id="purchase.app.type.select">
		<option value="purchase">Purchase</option>
		<option value="purchase-request">Purchase Request</option>
	</selection>
	
	<selection name="purchase.request.status.select">
		<option value="1">Draft</option>
		<option value="2">Requested</option>
		<option value="3">Accepted</option>
		<option value="4">Purchased</option>
		<option value="5">Partial delivery</option>
		<option value="6">Delivered</option>
		<option value="7">Refused</option>
		<option value="8">Canceled</option>
	</selection>
	
</object-views>

<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.4.xsd">

  <grid name="call-tender-mail-grid" title="Offer mail"
    model="com.axelor.apps.purchase.db.CallTenderMail">
    <field name="mailTemplate"/>
    <field name="metaFile"/>
    <field name="sentMessage"/>
  </grid>

  <form name="call-tender-mail-form" model="com.axelor.apps.purchase.db.CallTenderMail"
    title="Supplier offer">
    <panel name="mainPanel" showTitle="false">
      <field name="mailTemplate" colSpan="6"/>
      <field name="metaFile" widget="binary-link" colSpan="6"/>
      <field name="sentMessage"/>
    </panel>

  </form>

</object-views>

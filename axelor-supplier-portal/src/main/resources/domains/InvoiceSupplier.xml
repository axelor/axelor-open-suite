<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_6.1.xsd">

  <module name="supplierportal" package="com.axelor.apps.supplierportal.db"/>

  <entity name="InvoiceSupplier">

    <many-to-one name="partner" ref="com.axelor.apps.base.db.Partner" title="Partner"/>
    <date name="invoiceDate" title="Invoice date"/>
    <date name="dueDate" title="Due date"/>
    <many-to-one name="invoiceFile" ref="com.axelor.meta.db.MetaFile" title="Invoice"/>

  </entity>

</domain-models>

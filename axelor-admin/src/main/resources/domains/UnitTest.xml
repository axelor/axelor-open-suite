<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="base" package="com.axelor.apps.base.db"/>

  <entity name="UnitTest" lang="java" cacheable="true">

    <string name="name" title="Name"/>
    <string name="testScript" title="Test script" large="true"/>
    <string name="testResult" title="Result" large="true" readonly="true"/>
    <datetime name="testRunOn" title="Run on" readonly="true"/>
    <one-to-many name="testLineList" title="Test lines"
      ref="com.axelor.apps.base.db.UnitTestLine" mappedBy="unitTest"/>

  </entity>

</domain-models>

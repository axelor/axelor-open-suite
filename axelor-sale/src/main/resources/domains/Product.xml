<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.3.xsd">
 
  <module name="base" package="com.axelor.apps.base.db"/>
 
  <entity name="Product" lang="java">
  
    <many-to-one name="salesUnit" ref="com.axelor.apps.base.db.Unit" title="Sales unit" initParam="true" massUpdate="true"/>
    <one-to-one name="configurator" ref="com.axelor.apps.sale.db.Configurator" orphanRemoval="true" mappedBy="product"/>
  	<one-to-many name="customerCatalogList" ref="com.axelor.apps.sale.db.CustomerCatalog" mappedBy="product" title="Customer catalog lines" />
  	
  </entity>

</domain-models>

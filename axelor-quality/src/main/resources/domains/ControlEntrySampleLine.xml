<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_6.1.xsd">

  <module name="quality" package="com.axelor.apps.quality.db"/>

  <entity name="ControlEntrySampleLine" cacheable="true">
    <integer name="resultSelect" selection="quality.control.entry.sample.line.result.select"
      title="Result"/>
    <many-to-one name="controlPlanLine" ref="com.axelor.apps.quality.db.ControlPlanLine"
      title="Control plan line"/>
    <many-to-one name="controlEntrySample"
      ref="com.axelor.apps.quality.db.ControlEntrySample" title="Control entry sample"/>
    <string name="log" title="Log"/>

    <extra-code>
			<![CDATA[
				public static final int RESULT_COMPLIANT = 2;
				public static final int RESULT_NOT_COMPLIANT = 3;
			]]>
    </extra-code>
  </entity>

</domain-models>
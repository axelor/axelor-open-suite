<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_7.4.xsd">

  <module name="stock" package="com.axelor.apps.stock.db"/>

  <entity name="TrackingNumber">

    <integer name="conformitySelect" selection="quality.tracking.number.conformity.select"
      title="Conformity"/>
    <one-to-many name="trackingNumberCharacteristicList"
      ref="com.axelor.apps.quality.db.TrackingNumberCharacteristic" title="Characteristics"
      mappedBy="trackingNumber"/>

    <extra-code>
			<![CDATA[
				public static final int CONFORMITY_COMPLAINT = 1;
				public static final int CONFORMITY_NON_COMPLAINT = 2;
				public static final int CONFORMITY_NOT_ASSESED = 3;
			]]>
    </extra-code>

  </entity>

</domain-models>

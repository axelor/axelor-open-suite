<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_6.1.xsd">

  <module name="quality" package="com.axelor.apps.quality.db"/>

  <entity name="QIAnalysisMethod">
    <string name="code" title="Code" required="true"/>
    <string name="name" title="Name" required="true"/>
    <boolean name="isCritical" title="Critical"/>
    <boolean name="isMajor" title="Major"/>
    <boolean name="isMinor" title="Minor"/>
    <boolean name="isProduct" title="Product"/>
    <boolean name="isSystem" title="System"/>
    <one-to-many ref="com.axelor.apps.quality.db.QIAnalysisMethodItem"
      name="qiAnalysisMethodItemList" title="Items" mappedBy="analysisMethod"/>
    <many-to-one ref="com.axelor.apps.base.db.Company" name="company" title="Company"
      required="true"/>
  </entity>

</domain-models>
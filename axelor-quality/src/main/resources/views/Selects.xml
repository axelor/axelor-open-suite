<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.4.xsd">

  <selection name="quality.alert.priority.select">
    <option color="green" value="1">Low</option>
    <option color="blue" value="2">Normal</option>
    <option color="orange" value="3">High</option>
    <option color="red" value="4">Urgent</option>
  </selection>

  <selection name="quality.alert.status.select">
    <option value="1">New</option>
    <option value="2">Confirmed</option>
    <option value="3">Action proposed</option>
    <option value="4">Solved</option>
  </selection>

  <selection name="quality.control.status.select">
    <option order="0" value="1">Draft</option>
    <option order="4" value="2">Canceled</option>
    <option order="1" value="3">Planned</option>
    <option order="2" value="4">In progress</option>
    <option order="3" value="5">Standby</option>
    <option order="5" value="6">Finished</option>
  </selection>

  <selection name="quality.control.plan.status.select">
    <option value="1" color="blue">Draft</option>
    <option value="2" color="purple">Validated</option>
    <option value="3" color="green">Applicable</option>
    <option value="4" color="red">Obsolete</option>
  </selection>

  <selection name="quality.control.entry.status.select">
    <option value="1" color="blue">Draft</option>
    <option value="2" color="orange">In progress</option>
    <option value="3" color="green">Finished</option>
    <option value="4" color="red">Canceled</option>
  </selection>

  <selection name="quality.control.plan.frequency.type.select">
    <option value="1">Sample</option>
    <option value="2">Systematic</option>
  </selection>

  <selection name="quality.control.entry.plan.line.type.select">
    <option value="1">Plan line</option>
    <option value="2">Entry sample line</option>
  </selection>

  <selection name="quality.control.entry.sample.result.select">
    <option value="1">Not controlled</option>
    <option value="2">Compliant</option>
    <option value="3">Not compliant</option>
  </selection>

  <selection name="quality.control.points.status.select">
    <option value="1">On hold</option>
    <option value="2">Done</option>
  </selection>

  <selection name="quality.control.point.model.period">
    <option value="1">Week(s)</option>
  </selection>

  <selection name="sequence.generic.code.select" id="quality.sequence.generic.code.select">
    <option value="qualityAlert">Quality alert</option>
    <option value="qualityControl">Quality control</option>
    <option value="qualityImprovement">Quality improvement</option>
    <option value="qiActionDistribution">QI Action distribution</option>
    <option value="qiDecisionDistribution">QI Decision distribution</option>
    <option value="requiredDocument">Required document</option>
  </selection>

  <selection name="team.type.select" id="quality.team.type.select">
    <option value="quality">Quality</option>
  </selection>

  <selection name="quality.measuring.point.measuring.type.select">
    <option value="1">Note</option>
    <option value="2">Conformity</option>
  </selection>

  <selection name="quality.qi.type.select">
    <option value="1">Product</option>
    <option value="2">System</option>
  </selection>

  <selection name="quality.qi.detection.origin.select">
    <option value="1">Supplier</option>
    <option value="2">Internal</option>
    <option value="3">Customer</option>
  </selection>

  <selection name="quality.qi.resolution.default.identification.select">
    <option value="1">Manual</option>
    <option value="2">Tracking Number</option>
  </selection>

  <selection name="quality.qi.resolution.decision.responsible.select">
    <option value="1">Employee</option>
    <option value="2">Customer</option>
    <option value="3">Supplier</option>
    <option value="4">Customer contact</option>
    <option value="5">Supplier contact</option>
  </selection>

  <selection name="quality.qi.resolution.decision.acceptance.select">
    <option value="1"></option>
    <option value="2">Waiting</option>
    <option value="3">Accepted</option>
    <option value="4">Rejected</option>
    <option value="5">Partially accepted</option>
  </selection>

  <selection name="quality.qi.gravity.type.select">
    <option value="1">Critical</option>
    <option value="2">Major</option>
    <option value="3">Minor</option>
  </selection>
  <selection name="quality.qi.analysis.efficiency.select">
    <option value="1">No checked</option>
    <option value="2">Yes</option>
    <option value="3">No</option>
  </selection>

  <selection name="quality.required.document.status.select">
    <option value="1">Required</option>
    <option value="2">Available</option>
    <option value="3">Obsolete</option>
  </selection>

  <selection name="quality.required.document.type.select">
    <option value="1">Sale order</option>
    <option value="2">Project</option>
  </selection>

  <selection name="quality.required.document.related.to.select">
    <option value="com.axelor.apps.base.db.Product">Product</option>
    <option value="com.axelor.apps.purchase.db.PurchaseOrder">Purchase order</option>
    <option value="com.axelor.apps.production.db.ManufOrder">ManufOrder</option>
  </selection>

  <selection name="quality.control.origin.related.to.select">
    <option value="com.axelor.apps.base.db.Product" data-grid="product-grid"
      product-form="invoice-form">Product</option>
    <option value="com.axelor.apps.base.db.Partner" data-grid="partner-grid"
      data-form="partner-form" data-domain="self.isSupplier = true">Partner</option>
    <option value="com.axelor.apps.project.db.Project" data-grid="project-grid"
      data-form="project-form">Project</option>
  </selection>

  <selection name="quality.control.plan.related.to.select">
    <option value="com.axelor.apps.base.db.Product">Product</option>
    <option value="com.axelor.apps.production.db.ProdProcessLine">ProdProcessLine</option>
  </selection>

  <selection name="quality.control.entry.related.to.select">
    <option value="com.axelor.apps.stock.db.StockMove">StockMove</option>
    <option value="com.axelor.apps.stock.db.StockMoveLine">StockMoveLine</option>
    <option value="com.axelor.apps.production.db.ManufOrder">ManufOrder</option>
    <option value="com.axelor.apps.production.db.OperationOrder">OperationOrder</option>
    <option value="com.axelor.apps.base.db.Product">Product</option>
    <option value="com.axelor.apps.stock.db.TrackingNumber">TrackingNumber</option>
  </selection>

  <selection name="quality.control.entry.operation.order.related.to.select">
    <option value="com.axelor.apps.production.db.OperationOrder">OperationOrder</option>
  </selection>

  <selection name="quality.characteristic.type.characteristic.type.select">
    <option value="1">Measurable</option>
    <option value="2">List</option>
    <option value="3">Comment</option>
  </selection>

  <selection name="quality.tracking.number.conformity.select">
    <option value="1">Compliant</option>
    <option value="2">Non-compliant</option>
    <option value="3">Not assessed</option>
  </selection>

</object-views>

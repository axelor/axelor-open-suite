<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_7.4.xsd">

  <module package="com.axelor.apps.budget.db" name="budget"/>

  <entity name="BudgetScenarioLine">
    <many-to-one name="budgetScenario" ref="com.axelor.apps.budget.db.BudgetScenario"/>
    <many-to-one name="budgetScenarioVariable"
      ref="com.axelor.apps.budget.db.BudgetScenarioVariable"/>
    <decimal name="year1Value" title="Starting Year" precision="20" scale="3"/>
    <decimal name="year2Value" title="Year 2" precision="20" scale="3"/>
    <decimal name="year3Value" title="Year 3" precision="20" scale="3"/>
    <decimal name="year4Value" title="Year 4" precision="20" scale="3"/>
    <decimal name="year5Value" title="Year 5" precision="20" scale="3"/>
    <decimal name="year6Value" title="Year 6" precision="20" scale="3"/>

  </entity>

</domain-models>

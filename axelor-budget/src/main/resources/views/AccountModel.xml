<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">

  <grid model="com.axelor.apps.budget.db.AccountModel" title="Account models"
    name="account-model-grid">
    <field name="code"/>
    <field name="name"/>
  </grid>

  <form model="com.axelor.apps.budget.db.AccountModel" title="Account model"
    name="account-model-form" width="large">
    <panel name="mainPanel">
      <field name="code"/>
      <field name="name"/>
    </panel>
  </form>

</object-views>

<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">

  <grid name="budget-scenario-line-grid" title="Budget Scenario Line"
    model="com.axelor.apps.budget.db.BudgetScenarioLine">
    <field name="budgetScenarioVariable"/>
    <field name="year1Value"/>
    <field name="year2Value"/>
    <field name="year4Value"/>
    <field name="year5Value"/>
    <field name="year6Value"/>
  </grid>

  <form model="com.axelor.apps.budget.db.BudgetScenarioLine" title="Budget Scenario Line"
    name="budget-scenario-line-form" width="large">
    <panel name="mainPanel" colSpan="12">

      <field name="budgetScenarioVariable" domain="self.entryMethod=0"/>
      <field name="year1Value"/>
      <field name="year2Value"/>
      <field name="year4Value"/>
      <field name="year5Value"/>
      <field name="year6Value"/>

    </panel>
  </form>
</object-views>

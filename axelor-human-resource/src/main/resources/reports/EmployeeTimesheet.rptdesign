            <method name="fetch"><![CDATA[if(timesheetList.hasNext()){
node = timesheetList.next();
var round = reportContext.getPersistentGlobalVariable("round");
	row["date"] = node.get("date");
	row["userName"] = node.get("userName").toString();
	row["workedHour"] = round(node.get("workedHour"),2);
	row["workingHour"] = round(node.get("workingHour"),2);
	row["weekNumber"] = round(node.get("weekNumber"),2);
	row["weeklyWorkHours"] = round(node.get("weeklyWorkHours"),2);
	return true;
}

return false;]]></method>

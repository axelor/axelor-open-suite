<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_6.1.xsd">

  <module name="studio" package="com.axelor.studio.db"/>

  <entity name="App" cacheable="true">

    <one-to-one ref="com.axelor.studio.db.AppEmployee" name="appEmployee" mappedBy="app"/>
    <one-to-one ref="com.axelor.studio.db.AppExpense" name="appExpense" mappedBy="app"/>
    <one-to-one ref="com.axelor.studio.db.AppLeave" name="appLeave" mappedBy="app"/>
    <one-to-one ref="com.axelor.studio.db.AppTimesheet" name="appTimesheet"
      mappedBy="app"/>

  </entity>

</domain-models>

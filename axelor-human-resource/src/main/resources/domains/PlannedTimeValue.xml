<?xml version="1.0" ?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_7.0.xsd">

  <module name="project" package="com.axelor.apps.project.db"/>

  <entity name="PlannedTimeValue" cacheable="true">
    <string name="name" unique="true">
      <![CDATA[
		        String name = "";
	            if(plannedTime != null) name = plannedTime.toString();
	            return name;
            ]]>
    </string>
    <decimal name="plannedTime" title="Planned time"/>
  </entity>

</domain-models>
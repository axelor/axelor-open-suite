<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">

  <grid name="expense-limit-grid" title="Expense limits"
    model="com.axelor.apps.hr.db.ExpenseLimit">
    <field name="fromDate"/>
    <field name="toDate"/>
    <field name="maxAmount"/>
  </grid>

  <form name="expense-limit-form" title="Expense limit"
    model="com.axelor.apps.hr.db.ExpenseLimit" width="large">
    <panel name="mainPanel" itemSpan="4">
      <field name="fromDate"/>
      <field name="toDate"/>
      <field name="maxAmount"/>
    </panel>
  </form>

</object-views>

<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">

  <grid name="employment-contract-sub-type-grid" title="Contract sub type"
    model="com.axelor.apps.hr.db.EmploymentContractSubType">
    <field name="code"/>
    <field name="description"/>
  </grid>

  <form name="employment-contract-sub-type-form" title="Contract sub type"
    model="com.axelor.apps.hr.db.EmploymentContractSubType">
    <panel name="mainPanel">
      <field name="code"/>
      <field name="description" widget="string"/>
    </panel>
  </form>

</object-views>
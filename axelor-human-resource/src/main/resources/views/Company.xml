	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

	<form name="company-form" title="Company" model="com.axelor.apps.base.db.Company" extension="true">
		<extend target="//panel[@name='configurationsPanel']/*[last()]">
			<insert position="after">
				<field name="employees" colSpan="2" />
			</insert>
		</extend>
	</form>

</object-views>
<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.1.xsd">
    
    <grid name="kilometric-allow-param-grid" title="Kilometrice Allowance Params" model="com.axelor.apps.hr.db.KilometricAllowParam" orderBy="code">
        <field name="name"/>
	    <field name="code" x-bind="{{code|unaccent|uppercase}}"/>
    </grid>

	<form name="kilometric-allow-param-form" title="Kilometrice Allowance Param" model="com.axelor.apps.hr.db.KilometricAllowParam" width="large">
	  	<panel name="mainPanel">
	    	<field name="name"/>
	    	<field name="code" x-bind="{{code|unaccent|uppercase}}"/>
	  	</panel>
	</form>
	
</object-views>

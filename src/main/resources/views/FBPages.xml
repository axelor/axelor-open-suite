<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_0.9.xsd">
	<grid title="Page" name="fb-page-grid" model="com.axelor.apps.base.db.FBPages">
		<toolbar>
			<button name="new" title="New" hidden="true" />
			<button name="edit" title="Edit" hidden="true" />
			<button name="save" title="Save" hidden="true" />
			<button name="firePages" title="Page(s)"
				onClick="com.axelor.sn.web.FBController:getPages" />
		</toolbar>

		<field name="name"></field>
		<field name="username"></field>
		<field name="pageUrl"></field>
		<field name="pageId" hidden="true"></field>
		<field name="curUser" hidden="true"></field>
	</grid>

	<form title="Page Post" name="fb-page-form" model="com.axelor.apps.base.db.FBPages">
		<field name="postcontent"></field>
	</form>
</object-views>

<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_2.1.xsd">
    <grid name="analytic-journal-grid" title="Analytic Journal" model="com.axelor.apps.account.db.AnalyticJournal">
        <field name="name"/>
        <field name="type"/>
        <field name="company"/>
    </grid>
    <form name="analytic-journal-form" title="Analytic Journal" model="com.axelor.apps.account.db.AnalyticJournal"  >
        <panel title="Overview">
        	<field name="name"/>
        	<field name="type"/>
        	<field name="company" widget="SuggestBox"/>
        </panel>
        
    </form>
</object-views>
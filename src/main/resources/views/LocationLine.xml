<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_0.9.xsd">
    
    <grid name="location-line-grid" title="Stock Locations content" model="com.axelor.apps.supplychain.db.LocationLine">
        <field name="product"/>
        <field name="currentQty"/>
        <field name="futureQty"/>
    </grid>
    
    <form name="location-line-form" title="Stock Location content" model="com.axelor.apps.supplychain.db.LocationLine" cols="4">
        <field name="product"/>
        <field name="currentQty"/>
        <field name="futureQty"/>
    </form>
    
    <grid name="location-line-product-grid" title="Stock Locations content" model="com.axelor.apps.supplychain.db.LocationLine">
        <field name="location"/>
        <field name="currentQty"/>
        <field name="futureQty"/>
        <field name="lastFutureStockMoveDate"/>
    </grid>
    
    <form name="location-line-product-form" title="Stock Location content" model="com.axelor.apps.supplychain.db.LocationLine" cols="4">
        <field name="location"/>
        <field name="currentQty"/>
        <field name="futureQty"/>
        <field name="lastFutureStockMoveDate"/>
    </form>
</object-views>

<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_0.9.xsd">

	<grid title="Follower Request" name="twt-follower-grid" model="com.axelor.apps.base.db.TwitterFollowerRequest">
		<toolbar>
			<button name="save" title="Save" hidden="true" />
			<button name="new" title="New" hidden="true" />
			<button name="edit" title="Edit" hidden="true" />
			<button name="fireFetch" title="Follower Request"
				onClick="com.axelor.sn.web.TWTController:getFollowerRequest" />
		</toolbar>
		<field name="sendedOn"></field>
		<field name="screenName"></field>
		<field name="name"></field>
		<field name="link"></field>
		<field name="userId" hidden="true"></field>
	</grid>

	<form title="Follower Detail" name="twt-follower-form" model="com.axelor.apps.base.db.TwitterFollowerRequest">
		<field name="sendedOn"></field>
		<field name="screenName"></field>
		<field name="name"></field>
		<field name="link"></field>
	</form>

</object-views>

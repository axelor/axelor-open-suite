<?xml version="1.0" encoding="UTF-8"?>
<csv-inputs xmlns="http://apps.axelor.com/xml/ns/data-import"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/data-import http://apps.axelor.com/xml/ns/data-import/data-import_0.8.xsd">
  
  	<input file="groups_AX.csv" type="com.axelor.auth.db.Group" />

	<input file="IndexEventParameter.csv" type="com.axelor.apps.event.db.IndexEventParameter" />
	
	<input file="Dial.csv" type="com.axelor.apps.event.db.Dial" />
	
	<input file="typereleve.csv" type="com.axelor.apps.territory.db.MeterReadingType">
		<bind to="koalaId" column="typerel_id"/>
		<bind to="name" column="typerel_lib"/>
	</input>

	<input file="AnalyticAxis.csv" type="com.axelor.apps.account.db.AnalyticAxis" />

	<input file="AnalyticAccount.csv" type="com.axelor.apps.account.db.AnalyticAccount" />
	
	<input file="ParameterType.csv" type="com.axelor.apps.pricing.db.ParameterType"/>
	
	<input file="Parameter.csv" type="com.axelor.apps.pricing.db.Parameter"/>
	
	<input file="zonemeteo.csv" type="com.axelor.apps.territory.db.WeatherZone">
		<bind to="importId" column="zonemeteo_id"/>
		<bind to="code" column="zonemeteo_code"/>
		<bind to="name" column="zonemeteo_lib"/>
	</input>
	
	<input file="typesectionfacture.csv" type="com.axelor.apps.invoice.db.InvoiceLineType">
		<bind to="koalaId" column="typesecfac_id"/>
		<bind to="code" column="typsecfac_libcourt"/>
		<bind to="name" column="typsecfac_liblong"/>
		<bind to="orderId" column="typsecfac_ord"/>
	</input>
	
	<input file="activite.csv" type="com.axelor.apps.contact.db.Activity">
		<bind column="activite_id" to="koalaId" />
		<bind column="activite_libelle" to="name" />
	</input>

	<input file="fluide.csv" type="com.axelor.apps.fluid.db.Fluid">
		<bind column="fluide_id" to="koalaId" />
		<bind column="fluide_nom" to="name" />
		<bind column="fluide_code" to="code" />
		<bind column="fluide_type_id" to="type" eval="fluide_type_id == '1'? 'elec' : fluide_type_id == '2'? 'gas': fluide_type_id == '3'? 'cable' : 'none'" />
	</input>
	
	<input file="rythmeflux.csv" type="com.axelor.apps.territory.db.FlowRate">
		<bind to="importId" column="rythmeflux_id"/>
		<bind to="fluid" column="rythmeflux_fluide_id" search="self.koalaId = :rythmeflux_fluide_id" update="true"/>
		<bind to="name" column="rythmeflux_lib"/>
	</input>

	<input file="secteuractivite.csv" type="com.axelor.apps.contact.db.NceCode">
		<bind to="koalaId" column="sectact_id"/>
		<bind to="code" column="sectact_code"/>
		<bind to="name" column="sectact_libelle"/>
	</input>
	
	<input file="naturedemande.csv" type="com.axelor.apps.crm.db.CrmRequestNature">
		<bind column="natdem_id" to="importId"/>
		<bind column="natdem_libelle" to="name"/>
	</input>

	<input file="typecompteur.csv" type="com.axelor.apps.territory.db.MeterType">
		<bind column="typecpt_id" to="koalaId" />
		<bind column="typecpt_lib" to="name" />
	</input>
	
	<input file="typeutilisateur.csv" type="com.axelor.apps.contract.db.UserType">
		<bind column="typeutil_lib" to="name" />
		<bind column="typeutil_id" to="koalaId" />
		<bind to="code" eval="!typeutil_lib?.empty ? (typeutil_lib.size() > 0 ? ((typeutil_lib.padRight(3))[0..2]).toUpperCase():null) : null" />
	</input>
	
	<input file="typememoire.csv" type="com.axelor.apps.invoice.db.InvoicesBillType">
		<bind to="koalaId" column="typemem_id"/>
		<bind to="name" column="typemem_libelle"/>
		<bind to="typeSelect" eval="typemem_id == '1' ? 'groupingFolder' : 'accountingDoc'"/>
	</input>
	
	<input file="MacroEventType.csv" type="com.axelor.apps.event.db.MacroEventType">
		<bind to="code" column="code"/>
		<bind to="name" column="name"/>
		<bind to="origin" column="origin"/>
		<bind to="sequence" eval="!code.empty ? (code=='MES' ? 0 : (code=='RES' ?2:1)):1"/>
		<bind to="description" column="description"/>
	</input>
	
	<input file="canalevt.csv" type="com.axelor.apps.crm.db.ActionEventSource">
		<bind to="koalaId" column="canevt_id"/>
		<bind to="name" column="canevt_liblong"/>
		<bind to="code" column="canevt_libcourt"/>
	</input>
	
	<input file="famevt.csv" type="com.axelor.apps.crm.db.ActionEventFamily">
		<bind to="koalaId" column="famevt_id"/>
		<bind to="name" column="famevt_liblong"/>
		<bind to="code" column="famevt_libcourt"/>
	</input>
	
	<input file="sousfamevt.csv" type="com.axelor.apps.crm.db.ActionEventSubFamily">
		<bind to="koalaId" column="sousfamevt_id"/>
		<bind to="actionEventFamily" search="self.koalaId = :sousfamevt_famevt_id" update="true"/>
		<bind to="name" column="sousfamevt_liblong"/>
		<bind to="code" column="sousfamevt_libcourt"/>
	</input>

	<input file="typechauffage.csv" type="com.axelor.apps.contract.db.HeatingType">
		<bind column="typechauff_id" to="koalaId" />
		<bind column="typechauff_libelle" to="name" />
	</input>
	
	<input file="typeecheancier.csv" type="com.axelor.apps.account.db.PaymentScheduleType">
		<bind column="typeech_libcourt" to="code" />
		<bind column="typeech_liblong" to="name" />
		<bind column="typeech_id" to="koalaIdType" />
		<bind to="type" eval="'type'"/>
	</input>
	
	<input file="soustypeecheancier.csv" type="com.axelor.apps.account.db.PaymentScheduleType">
		<bind column="soustypeech_libcourt" to="code" />
		<bind column="soustypeech_liblong" to="name" />
		<bind column="soustypeech_id" to="koalaIdSubType" />
		<bind to="type" eval="'subtype'"/>
	</input>
	
	<input file="typeecheancierAX.csv" type="com.axelor.apps.account.db.PaymentScheduleType" search="self.code = :code" update="true">
		<bind column="code" to="code" />
		<bind column="alertOk" to="alertOk" eval="alertOk == '1' ? 'true' : 'false'" />
		<bind column="alertMessage" to="alertMessage" />
	</input>
	
	<input file="motifbloquage.csv" type="com.axelor.apps.contract.db.StopReason"/>
	
	<input file="pays.csv" type="com.axelor.apps.territory.db.Country">
		<bind column="pays_id" to="koalaId"/>
		<bind column="pays_cog" to="cog" />
		<bind column="pays_nom" to="name" />
		<bind column="pays_abrev" to="code" />
		<bind column="pays_indic_tel" to="phonePrefix" />
	</input>

	<input file="region.csv" type="com.axelor.apps.territory.db.Region">
		<bind column="region_num" to="code" />
		<bind column="region_nom" to="name" />
		<bind search="self.koalaId = :region_pays_id" to="country" update="true"/>
	</input>
	
	<input file="adresse.csv" type="com.axelor.apps.contact.db.Address">
		<bind column="adresse_id" to="koalaId" />
		<bind column="adresse_L2" to="addressL2" />
		<bind column="adresse_L3" to="addressL3" />
		<bind column="adresse_L4" to="addressL4" />
		<bind column="adresse_L5" to="addressL5" />
		<bind column="adresse_L6" to="addressL6" />
		<bind column="adresse_lieuconso" to="consumptionPlaceOk" eval="adresse_lieuconso == '1'? 'true' : 'false'"/>
		<bind column="adresse_flag_certif" to="certifiedOk" eval="adresse_flag_certif == '1'? 'true' : 'false'" />
		<bind search="self.code = :adresse_pays_cod" to="addressL7Country" update="true"/>
		<bind to="inseeCode" column="adresse_INSEE"/>
	</input>
	
	<input file="civilite.csv" type="com.axelor.apps.contact.db.Title">
		<bind column="civilite_id" to="koalaId" />
		<bind column="civilite_libcourt" to="code" />
		<bind column="civilite_libelle" to="name" />
		<bind column="civilite_type" to="type" eval="civilite_type == 'F'? '2' : '1'" />
	</input>
	
	<input file="fonctioncontact.csv" type="com.axelor.apps.contact.db.Jobtitle">
		<bind column="fonction_id" to="koalaId" />
		<bind column="fonction_code" to="code" />
		<bind column="fonction_libellé" to="name" />
	</input>
	
	<input file="contact.csv" type="com.axelor.apps.contact.db.Contact">
		<bind column="contact_id" to="koalaId" />
		<bind column="contact_L1" to="addressL1" />
		<bind column="contact_nom" to="name" eval="!contact_nom ? 'Inconnu' : contact_nom"/>
		<bind column="contact_prenom" to="firstName" />
		<bind search="self.koalaId = :contact_civilite_id" to="title" update="true"/>
		<bind search="self.koalaId = :contact_fonction_id" to="jobtitle" update="true"/>
		<bind column="contact_campagne_flag" to="noCampaignOk" eval="contact_campagne_flag == '1'? 'true' : 'false'" />
		<bind column="contact_telfixe_perso1" to="fixedPhone1" />
		<bind column="contact_telfixe_perso2" to="fixedPhone2" />
		<bind column="contact_telport_perso" to="mobilePhonePerso" />
		<bind column="contact_telfixe_prof" to="fixedPhonePro" />
		<bind column="contact_telport_prof" to="mobilePhonePro" />
		<bind column="contact_fax_prof" to="fax" />
		<bind column="contact_email" to="email" />
		<bind search="self.koalaId = :contact_adresse_id" to="address" update="true"/>
		<bind to="olaPhone" column="contact_tel_AEL"/>
		<bind to="olaEmail" column="contact_email_AEL"/>
		<bind to="webSite" column="contact_site_web"/>
	</input>
	
	<input file="accueil.csv" type="com.axelor.apps.contact.db.Agency">
		<bind column="accueil_id" to="importId" />
		<bind column="accueil_id" to="num" />
		<bind column="accueil_libelle_court" to="nameShort" />
		<bind column="accueil_libelle_long" to="nameLong" />
		<bind search="self.koalaId = :accueil_adresse_id" to="address" update="true"/>
		<bind column="accueil_rech1" to="code" />
		<bind to="openingHours" column="accueil_horaires"/>
	</input>
	
	<input file="accueil.csv" type="com.axelor.apps.contact.db.Agency"
		search="self.importId = :accueil_id" update="true">
		<bind column="accueil_id" to="importId" />
		<bind search="self.importId = :accueil_parent_id" to="parentAgency" update="true"/>
	</input>

	<input file="centreexploit.csv" type="com.axelor.apps.territory.db.OperationCenter">
		<bind column="centreexploit_id" to="koalaId" />
		<bind search="self.koalaId = :centrexploit_adresse_id" to="address" update="true"/>
		<bind search="self.koalaId = :centreexploit_parent_id" to="parent" update="true"/>
		<bind column="centreexploit_libcourt" to="code" />
		<bind column="centreexploit_nom" to="name" />
	</input>
	
	<input file="segment.csv" type="com.axelor.apps.pricing.db.Parameter">
		<bind to="parameterId" eval="!segment_id?.empty ?  'segtUtilisateur_'+segment_id : '' " />
		<bind to="name" column="segment_code" />
		<bind to="description" column="segment_code" />
		<bind to="parameterType" search="self.code = 'segtUtilisateur'" >
			<bind to="code" eval="'segtUtilisateur'" />
		</bind>
	</input>

	<input file="segment.csv" type="com.axelor.apps.contract.db.UserSegment">
		<bind column="segment_id" to="koalaId" />
		<bind column="segment_code" to="code" />
		<bind column="segment_libelle" to="name" />
		<bind search="self.koalaId = :segment_typefluide_id" to="fluid" update="true"/>
		<bind search="self.koalaId = :segment_typeutil_id" to="userType" update="true"/>
		<bind search="self.parameterId = :segment_typecontrat_id" to="contractFormatParameter" update="true">
			<bind column="segment_typecontrat_id" to="parameterId" eval="segment_typecontrat_id == '6' ? 'formCont_CARD' : segment_typecontrat_id == '3' ? 'formCont_CU' : null" />
		</bind>
		<bind search="self.name = :segment_code and self.parameterType.code = 'segtUtilisateur'" to="userSegmentParameter" update="true"/>
	</input>

	<input file="typecollectivite.csv" type="com.axelor.apps.contact.db.AuthorityType">
		<bind column="typecollectivite_id" to="koalaId" />
		<bind column="typecollectivite_libelle" to="name" />
	</input>

	<input file="naturetierstit.csv" type="com.axelor.apps.contact.db.ClientNature">
		<bind column="nattierstit_id" to="koalaId" />
		<bind column="nattierstit_libelle" to="name" />
		<bind to="code" eval="nattierstit_libelle.size() > 0 ? ((nattierstit_libelle.padRight(3))[0..2]).toUpperCase():null" />
		<bind to="agentOk" eval="nattierstit_id=='5'?'true':'false'"/>
	</input>

	<input file="zoneinfluence.csv" type="com.axelor.apps.contact.db.CatchmentArea">
		<bind column="zoneinf_id" to="koalaId" />
		<bind column="zoneinf_nom" to="name" />
		<bind column="zoneinf_flag_elec" to="elecOk" eval="zoneinf_flag_elec == '1'? 'true' : 'false'" />
		<bind column="zoneinf_flag_gaz" to="gasOk" eval="zoneinf_flag_gaz == '1'? 'true' : 'false'" />
	</input>
	
	<input file="apenaf.csv" type="com.axelor.apps.contact.db.NafCode">
		<bind to="koalaId" column="APENAF_id"/>
		<bind to="code" column="APENAF_code"/>
		<bind to="name" column="APENAF_libelle"/>
	</input>
	
	<input file="tiers.csv" type="com.axelor.apps.contact.db.Partner">
		<bind column="tiers_id" to="koalaId" />
		<bind column="tiers_crit_rech1" to="importKey"/>
		<bind search="self.koalaId = :tiers_contact1_id" to="mainContact" update="true"/>
		<bind to="personCategorySelect" eval="tiers_typepersonne_type == 'P' ? 'naturalPerson' : 'legalPerson'" />
		<bind column="tiers_enseigne" to="corporateBrand" />
		<bind to="nafCode" search="self.code = :tiers_NAF" update="true" if="!tiers_NAF?.empty"/>
		<bind to="nceCode" search="self.code = :tiers_NCE" update="true" if="!tiers_NCE?.empty"/>
		<bind column="tiers_COFACE" to="cofaceNote" />
		<bind column="tiers_TVA_Intracomm" to="intraCommunityVat" />
		<bind column="tiers_flag_tit" to="clientOk" eval="tiers_flag_tit == '1'? 'true' : 'false'" />
		<bind column="tiers_flag_pay" to="payerOk" eval="tiers_flag_pay == '1'? 'true' : 'false'" />
		<bind column="tiers_flag_prop" to="ownerOk" eval="tiers_flag_prop == '1'? 'true' : 'false'" />
		<bind column="tiers_flag_tuteur" to="guardianOk" eval="tiers_flag_tuteur == '1'? 'true' : 'false'" />
		<bind column="tiers_flag_fourn" to="providerOk" eval="tiers_flag_fourn == '1'? 'true' : 'false'" />
		<bind column="tiers_flag_GRD" to="dsoOk" eval="tiers_flag_GRD == '1'? 'true' : 'false'" />
		<bind column="tiers_flag_apportaff" to="businessGeneratorOk" eval="tiers_flag_apportaff == '1'? 'true' : 'false'" />
		<bind column="tiers_flag_prospect" to="prospectOk" eval="tiers_flag_prospect == '1'? 'true' : 'false'" />
		<bind column="tiers_flag_syndic" to="propertyAgentOk" eval="tiers_flag_syndic == '1'? 'true' : 'false'" />
		<bind column="tiers_flag_collectivite" to="authorityOk" eval="tiers_flag_collectivite == '1'? 'true' : 'false'" />
		<bind column="tiers_flag_treso" to="treasuryOk" eval="tiers_flag_treso == '1'? 'true' : 'false'" />
		<bind column="tiers_flag_RE" to="breOk" eval="tiers_flag_RE == '1'? 'true' : 'false'" />
		<bind column="tiers_flag_parten" to="partnerOk" eval="tiers_flag_parten == '1'? 'true' : 'false'" />
		<bind column="tiers_flag_servicesocial" to="socialServiceOk" eval="tiers_flag_servicesocial == '1'? 'true' : 'false'" />
		<bind column="tiers_flag_concedant" to="assignorOk" eval="tiers_flag_concedant == '1'? 'true' : 'false'" />
		<bind column="tiers_flag_commercialisateur" to="marketerOk" eval="tiers_flag_commercialisateur == '1'? 'true' : 'false'" />
		<bind column="tiers_flag_huissier" to="usherOk" eval="tiers_flag_huissier == '1'? 'true' : 'false'" />
		<bind column="tiers_flag_centreIEG" to="iegOk" eval="tiers_flag_centreIEG == '1'? 'true' : 'false'" />
		<bind column="tiers_flag_utilGRD" to="userOk" eval="tiers_flag_utilGRD == '1'? 'true' : 'false'" />
		<bind column="tiers_SIRET" to="longCompanyId" eval="(!tiers_SIRET?.empty &amp;&amp; tiers_SIRET.length() == 14) ? tiers_SIRET : null"/>
		<bind column="tiers_SIRENE" to="shortCompanyId" eval="(!tiers_SIRENE?.empty &amp;&amp; tiers_SIRENE.length() == 9) ? tiers_SIRENE : null"/>
	</input>
	
	<input file="tiersgrd.csv" type="com.axelor.apps.contact.db.Partner" update="true" search="self.koalaId = :tiersgrd_id">
		<bind search="self.koalaId = :tiersgrd_contact1_id" to="dsoContact" update="true"/>
		<bind search="self.koalaId = :tiersgrd_contact2_id" to="dsoContactSet" update="true"/>
		<bind search="self.koalaId = :tiersgrd_contact3_id" to="dsoContactSet" update="true"/>
		<bind search="self.koalaId = :tiersgrd_contact4_id" to="dsoContactSet" update="true"/>
		<bind column="tiersgrd_codeEIC" to="dsoNum" />
		<bind to="dsoCodeParameter" search="self.name = :tiersgrd_codeGRD and self.parameterType.code = 'grd'" >
			<bind to="parameterId" eval="!tiersgrd_codeGRD.empty ?  'grd_'+tiersgrd_codeGRD : '' " />
			<bind to="name" column="tiersgrd_codeGRD" />
			<bind to="description" column="tiersgrd_codeGRD" />
			<bind to="parameterType" search="self.code = 'grd'" >
				<bind to="code" eval="!tiersgrd_codeGRD.empty ?  'grd' : '' " />
			</bind>
		</bind>
	</input>	
	
	<input file="tiersappaff.csv" type="com.axelor.apps.contact.db.Partner" update="true" search="self.koalaId = :tiersappaff_id">
		<bind search="self.koalaId = :tiersappaff_contact1_id" to="businessGetterMainContact" update="true"/>
		<bind search="self.koalaId = :tiersappaff_zone_influence_id" to="catchmentArea" update="true"/>
		<bind search="self.koalaId = :tiersappaff_contact2_id" to="businessGetterContactSet" column="tiersappaff_contact2_id" />
		<bind search="self.koalaId = :tiersappaff_contact3_id" to="businessGetterContactSet" column="tiersappaff_contact3_id" />
		<bind search="self.koalaId = :tiersappaff_contact4_id" to="businessGetterContactSet" column="tiersappaff_contact4_id" />
	</input>

	<input file="tierscoll.csv" type="com.axelor.apps.contact.db.Partner" update="true" search="self.koalaId = :tierscoll_id">
		<bind search="self.koalaId = :tierscoll_typecollectivite_id" to="authorityType" update="true"/>
		<bind search="self.koalaId = :tierscoll_tierstreso_id" to="treasuryPartner" update="true"/>
		<bind search="self.koalaId = :tierscoll_contact1_id" to="authorityMainContact" update="true"/>
		<bind column="tierscoll_flag_conventionEP" to="epConventionOk" eval="tierscoll_flag_conventionEP == '1' ? 'true' : 'false'" />
		<bind search="self.koalaId = :tierscoll_contact2_id" to="authorityContactSet" column="tierscoll_contact2_id" />
		<bind search="self.koalaId = :tierscoll_contact3_id" to="authorityContactSet" column="tierscoll_contact3_id" />
		<bind search="self.koalaId = :tierscoll_contact4_id" to="authorityContactSet" column="tierscoll_contact4_id" />
		<bind column="tierscoll_codeAGRESSO" to="authorityAccountingCode"/>
	</input>

	<input file="tierscomm.csv" type="com.axelor.apps.contact.db.Partner" update="true" search="self.koalaId = :tierscomm_id">
		<bind search="self.koalaId = :tierscomm_contact1_id" to="marketerMainContact" update="true"/>
		<bind search="self.koalaId = :tierscomm_contact2_id" to="marketerContactSet" column="tierscomm_contact2_id" />
		<bind search="self.koalaId = :tierscomm_contact3_id" to="marketerContactSet" column="tierscomm_contact3_id" />
		<bind search="self.koalaId = :tierscomm_contact4_id" to="marketerContactSet" column="tierscomm_contact4_id" />
		<bind to="marketerSiOk" column="tierscomm_flag_dansSI" eval="tierscomm_flag_dansSI == '1' ? true : false"/>
	</input>

	<input file="tiersconced.csv" type="com.axelor.apps.contact.db.Partner" update="true" search="self.koalaId = :tiersconced_id">
		<bind search="self.koalaId = :tiersconced_contact1_id" to="assignorMainContact" update="true"/>
		<bind search="self.koalaId = :tiersconced_typecollectivite_id" to="assignorAuthorityType" update="true"/>
		<bind search="self.koalaId = :tiersconced_contact2_id" to="assignorContactSet" column="tiersconced_contact2_id" />
		<bind search="self.koalaId = :tiersconced_contact3_id" to="assignorContactSet" column="tiersconced_contact3_id" />
		<bind search="self.koalaId = :tiersconced_contact4_id" to="assignorContactSet" column="tiersconced_contact4_id" />
		<bind column="tiersconced_code" to="assignorCode"/>
	</input>

	<input file="tiersieg.csv" type="com.axelor.apps.contact.db.Partner" update="true" search="self.koalaId = :tiersIEG_id">
		<bind search="self.koalaId = :tiersIEG_contact1_id" to="iegMainContact" update="true"/>
		<bind to="iegMainOk" eval="'true'" if="tiersIEG_id=='37012'"/><!-- IF tiers SOREGIES -->
		<bind column="tiersIEG_Code_Centre" to="iegCode" />
		<bind search="self.koalaId = :tiersIEG_contact2_id" to="iegContactSet" column="tiersIEG_contact2_id" />
		<bind search="self.koalaId = :tiersIEG_contact3_id" to="iegContactSet" column="tiersIEG_contact3_id" />
		<bind search="self.koalaId = :tiersIEG_contact4_id" to="iegContactSet" column="tiersIEG_contact4_id" />

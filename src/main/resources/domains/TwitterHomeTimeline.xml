<?xml version="1.0" encoding="UTF-8"?>
 <domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">
	<module name="base" package="com.axelor.apps.base.db" />
	<entity name="TwitterHomeTimeline">
		<long name="timelineContentId" title="Content ID" hidden="true"></long>
		<string name="screenName" title="Screen Name of User"></string>
		<string name="actualName" title="Name of User"></string>
		<string name="actualContent" multiline="true" title="Content of Tweet"></string>
		<string name="contentDate" title="Created Date"></string>	
		<many-to-one name="curUser" ref="com.axelor.auth.db.User" hidden="true"></many-to-one>
	</entity>
	</domain-models>
	 
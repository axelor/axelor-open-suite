<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">
	
	<module name="base" package="com.axelor.apps.base.db" />
	
	<entity name="LinkedinParameters">
		<integer name="recordNumbers" title="Number of Records" min="0" max="100" />
		<integer name="days" title="Number of Days" min="0" max="15" />
		<many-to-one name="curUser" ref="com.axelor.auth.db.User" unique="true"/>
	</entity>
</domain-models>
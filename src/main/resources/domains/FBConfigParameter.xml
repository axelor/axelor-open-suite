<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.9.xsd">
	<module name="base" package="com.axelor.apps.base.db" />
	<entity name="FBConfigParameter">
		<integer name="paramLimit" title="Limit" min="0" max="200" help="Number of Records to Retrive From Facebook" />
		<integer name="paramSince" title="Since" min="0" max="15" help="Values in Days"></integer>
		<integer name="paramUntil" title="Until" min="0" max="2" help="Values in Days"></integer>
		<many-to-one name="userInfo" ref="com.axelor.apps.base.db.UserInfo" unique="true" help="Values in Days"></many-to-one>
	</entity>
</domain-models>

<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">

  <grid name="bank-order-file-format-country-grid" orderBy="country.name"
    title="Config. per country" model="com.axelor.apps.bankpayment.db.BankOrderFileFormatCountry"
    editable="true">
    <field name="country"/>
    <field name="receiverAddressRequired"/>
  </grid>

  <form name="bank-order-file-format-country-form" title="Config. per country"
    model="com.axelor.apps.bankpayment.db.BankOrderFileFormatCountry">
    <panel name="mainPanel" colSpan="12">
      <field name="country" required="true"/>
      <field name="receiverAddressRequired"/>
    </panel>
  </form>

</object-views>
<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">


  <grid name="ebics-transfer-state-grid" title="Ebics transfer state"
    model="com.axelor.apps.bankpayment.db.EbicsTransferState">
    <field name="transactionId"/>
    <field name="segmentNumber"/>
    <field name="numSegments"/>
    <field name="lastSegment"/>
  </grid>

  <form name="ebics-transfer-state-form" title="Ebics transfer state"
    model="com.axelor.apps.bankpayment.db.EbicsTransferState">

    <panel name="mainPanel">
      <field name="transactionId"/>
      <field name="segmentNumber"/>
      <field name="numSegments"/>
      <field name="lastSegment"/>
    </panel>

  </form>

</object-views>

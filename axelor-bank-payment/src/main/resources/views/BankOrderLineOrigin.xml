<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">
	
    <grid name="bank-order-line-origin-grid" model="com.axelor.apps.bankpayment.db.BankOrderLineOrigin" title="Origins">
        <field name="relatedToSelect"/>
        <field name="relatedToSelectName"/>
        <field name="relatedToSelectDate"/>
        <field name="relatedToSelectDueDate"/>
    </grid>
    
	<form name="bank-order-line-origin-form" model="com.axelor.apps.bankpayment.db.BankOrderLineOrigin" title="Origin">
	    <panel name="mainPanel">
			<field name="relatedToSelect" widget="RefSelect" x-related="relatedToSelectId" colSpan="12"/>
	    </panel>
	 </form>
	 
</object-views>
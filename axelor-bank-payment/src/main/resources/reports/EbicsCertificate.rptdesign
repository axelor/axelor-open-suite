            <property name="dataSource">Data Source</property>
            <list-property name="resultSet"/>
            <xml-property name="queryText"><![CDATA[select
certificate.id as "certId",
case when certificate.type_select = 'signature' then 'EbicsCertificateReport.signatureTitle'
      when certificate.type_select = 'authentication' then 'EbicsCertificateReport.authenticationTitle'
      when certificate.type_select = 'encryption' then 'EbicsCertificateReport.encryptionTitle'
 end as title,
 case when certificate.type_select = 'signature' then 'EbicsCertificateReport.signature'
      when certificate.type_select = 'authentication' then 'EbicsCertificateReport.authentication'
      when certificate.type_select = 'encryption' then 'EbicsCertificateReport.encryption'
 end as certificate_title,
 case when certificate.type_select = 'signature' then 'A005'
      when certificate.type_select = 'authentication' then 'X002'
      when certificate.type_select = 'encryption' then 'E002'
 end as certificate_type,
 case when certificate.type_select = 'signature' then 'EbicsCertificateReport.signatureHash'
      when certificate.type_select = 'authentication' then 'EbicsCertificateReport.authenticationHash'
      when certificate.type_select = 'encryption' then 'EbicsCertificateReport.encryptionHash'
 end as certificate_hash,
 bank.host_id,
 bank.name as "bank_name",
 language.code as "bank_language",
 ebics_user.user_id,
 partner.partner_id,
 case when certificate.type_select = 'signature' then 'EbicsCertificateReport.signatureVersion'
      when certificate.type_select = 'authentication' then 'EbicsCertificateReport.authenticationVersion'
      when certificate.type_select = 'encryption' then 'EbicsCertificateReport.encryptionVersion'
 end as version,
 certificate.pem_string,
 certificate.sha2has,
 certificate.issuer,
 certificate.subject,
 certificate.private_key,
 certificate.init_letter_edition_date        
from bankpayment_ebics_certificate certificate
left join bankpayment_ebics_user ebics_user on (ebics_user.id = ?)
left join bankpayment_ebics_partner partner on (partner.id = ebics_user.ebics_partner)
left join bankpayment_ebics_bank bank on (bank.id = partner.ebics_bank)
left join base_language language on (language.id = bank.language)
order by certificate.type_select desc;]]></xml-property>

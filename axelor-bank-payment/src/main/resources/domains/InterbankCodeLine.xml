<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_7.4.xsd">

  <module name="Account" package="com.axelor.apps.account.db"/>

  <entity name="InterbankCodeLine" cacheable="true">
    <finder-method name="findOperationCodeByCode" using="code"
      filter="self.code = :code AND self.interbankCode.typeSelect = 1"/>
    <finder-method name="findOperationCodeByCodeAndInterBankCode"
      using="code, interbankCode" filter="self.code = :code AND self.interbankCode = :interbankCode"/>
  </entity>

</domain-models>
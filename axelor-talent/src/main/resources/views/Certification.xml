<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.4.xsd">

  <grid name="certification-grid" title="Certifications"
    model="com.axelor.apps.talent.db.Certification">
    <field name="certificationName"/>
    <field name="issueDate"/>
  </grid>

  <form name="certification-form" title="Certification"
    model="com.axelor.apps.talent.db.Certification" width="large">
    <panel name="mainPanel">
      <field name="certificationName" required="true"/>
      <field name="issueDate"/>
    </panel>
  </form>

</object-views>

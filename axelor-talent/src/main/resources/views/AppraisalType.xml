<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">

  <grid name="appraisal-type-grid" title="Appraisal types"
    model="com.axelor.apps.talent.db.AppraisalType">
    <field name="name"/>
  </grid>

  <form name="appraisal-type-form" title="Appraisal type"
    model="com.axelor.apps.talent.db.AppraisalType" width="large">
    <panel name="namePanel">
      <field name="name"/>
    </panel>
  </form>

</object-views>

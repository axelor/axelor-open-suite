<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.3.xsd">

    <module name="payroll" package="com.axelor.apps.payroll.db"/>

    <entity name="GradePayrollData" lang="java" cacheable="true">

        <many-to-one name="payGrade" ref="com.axelor.apps.payroll.db.Paygrade" title="Grade"/>
        <many-to-many name="components" ref="com.axelor.apps.payroll.db.PayrollComponent" title="Payroll Components"/>
        <many-to-one name="company" ref="com.axelor.apps.base.db.Company" title="Company"/>

        <unique-constraint columns="payGrade,company"/>


    </entity>
</domain-models>
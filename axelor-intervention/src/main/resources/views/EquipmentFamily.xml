<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://axelor.com/xml/ns/object-views"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.1.xsd">

  <grid model="com.axelor.apps.intervention.db.EquipmentFamily" title="Equipment families"
    name="equipment-family-grid">
    <field name="code"/>
    <field name="name"/>
    <field name="numberOfMaintenanceHours"/>
  </grid>

  <form model="com.axelor.apps.intervention.db.EquipmentFamily" title="Equipment family"
    name="equipment-family-form" width="large">
    <panel name="mainPanel">
      <field name="code"/>
      <field name="name"/>
      <field name="numberOfMaintenanceHours"/>
    </panel>
  </form>

</object-views>

<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.4.xsd">

  <grid name="bill-of-material-import-source-grid" title="Bill of materials import sources"
    model="com.axelor.apps.production.db.BillOfMaterialImportSource">
    <field name="name"/>
  </grid>

  <form name="bill-of-material-import-source-form" title="Bill of materials import source"
    model="com.axelor.apps.production.db.BillOfMaterialImportSource">
    <panel name="mainPanel">
      <field name="name"/>
      <field name="bindingFile" widget="binary-link"/>
    </panel>
  </form>

</object-views>



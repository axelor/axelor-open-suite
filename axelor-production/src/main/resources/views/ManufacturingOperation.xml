<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">

  <calendar name="manufacturing-operation-planned-calendar"
    model="com.axelor.apps.production.db.ManufacturingOperation" eventStart="plannedStartDateT"
    eventStop="plannedEndDateT" title="Planned operations" colorBy="machine" mode="week"
    onChange="action-manufacturing-operation-group-set-planned-dates">
    <field name="name"/>
  </calendar>

  <calendar name="manufacturing-operation-real-calendar"
    model="com.axelor.apps.production.db.ManufacturingOperation" eventStart="realStartDateT"
    eventStop="realEndDateT" title="Real operations" colorBy="machine" mode="week"
    onChange="action-manufacturing-operation-method-set-real-dates">
    <field name="name"/>
  </calendar>

  <calendar name="planning-machine-calender"
    model="com.axelor.apps.production.db.ManufacturingOperation" title="Machine Planning"
    eventStart="plannedStartDateT" eventStop="plannedEndDateT" colorBy="statusSelect">
    <field name="operationName"/>
  </calendar>


  <grid name="manufacturing-operation-group-grid" title="Operations"
    model="com.axelor.apps.production.db.ManufacturingOperation" groupBy="manufOrder"
    orderBy="-manufOrder,priority" canNew="false">
    <toolbar>
      <button name="printBtn" title="Print" icon="fa-print"
        onClick="action-method-print-birt-list"/>
    </toolbar>
    <field name="manufOrder" width="120" grid-view="manuf-order-grid"
      form-view="manuf-order-form"/>
    <field name="priority"/>
    <field name="operationName" readonlyIf="statusSelect == 6"/>
    <field name="outsourcing"/>
    <field name="machine" grid-view="machine-grid" form-view="machine-form"/>
    <field name="plannedStartDateT"/>
    <field name="plannedEndDateT"/>
    <field name="realStartDateT"/>
    <field name="realEndDateT"/>
    <field name="statusSelect"/>
    <field name="prodProcessLine.optional" hidden="true"/>
    <field name="manufOrder.statusSelect" hidden="true"/>
    <field name="manufOrder.prodProcess" hidden="true"/>
    <field name="manufOrder.prodProcess.operationContinuity" hidden="true"/>
    <field name="manufOrder.manufacturingOperationMaxPriority" hidden="true"/>
    <button name="planBtn" icon="fa-calendar"
      onClick="action-manufacturing-operation-method-plan"
      readonlyIf="statusSelect != 2 || $get('manufOrder.statusSelect') &lt; 3"/>
    <button name="startBtn" icon="fa-play"
      onClick="action-manufacturing-operation-group-start-or-resume"
      readonlyIf="(statusSelect != 3 &amp;&amp; statusSelect != 5) || ($get('manufOrder.prodProcess.operationContinuity') &amp;&amp; !((!$get('prodProcessLine.optional') &amp;&amp; priority == $get('manufOrder.manufacturingOperationMaxPriority')) || ($get('prodProcessLine.optional') &amp;&amp; priority &lt; $get('manufOrder.manufacturingOperationMaxPriority'))))"/>
    <button name="pauseBtn" icon="fa-pause"
      onClick="action-manufacturing-operation-group-pause,save" readonlyIf="statusSelect != 4"/>
    <button name="finishBtn" icon="fa-power-off"
      onClick="action-manufacturing-operation-group-finish" readonlyIf="statusSelect != 4"/>
    <button name="cancelBtn" icon="fa-times-circle"
      onClick="action-manufacturing-operation-group-cancel,save"
      readonlyIf="statusSelect == 2 || statusSelect == 6"/>
    <button name="printBtn" title="Print" icon="fa-print" onClick="action-method-print-birt"/>
  </grid>


  <grid name="manufacturing-operation-grid" title="Operations"
    model="com.axelor.apps.production.db.ManufacturingOperation" orderBy="priority" canNew="false">
    <hilite if="outsourcing" color="orange"/>
    <field name="priority" width="80"/>
    <field name="operationName" readonlyIf="statusSelect == 6"/>
    <field name="machine" grid-view="machine-grid" form-view="machine-form"/>
    <field name="plannedStartDateT"/>
    <field name="plannedEndDateT"/>
    <field name="realStartDateT"/>
    <field name="realEndDateT"/>
    <field name="statusSelect"/>
    <field name="prodProcessLine.optional" hidden="true"/>
    <field name="manufOrder.statusSelect" hidden="true"/>
    <field name="manufOrder.prodProcess" hidden="true"/>
    <field name="outsourcing" hidden="true"/>
    <field name="manufOrder.prodProcess.operationContinuity" hidden="true"/>
    <field name="manufOrder.manufacturingOperationMaxPriority" hidden="true"/>
    <button name="planBtn" icon="fa-calendar"
      onClick="save,action-manufacturing-operation-method-plan"
      readonlyIf="statusSelect != 2 || $get('manufOrder.statusSelect') &lt; 3"/>
    <button name="startBtn" icon="fa-play"
      onClick="save,action-manufacturing-operation-group-start-or-resume"
      readonlyIf="(statusSelect != 3 &amp;&amp; statusSelect != 5) || ($get('manufOrder.prodProcess.operationContinuity') &amp;&amp; !((!$get('prodProcessLine.optional') &amp;&amp; priority == $get('manufOrder.manufacturingOperationMaxPriority')) || ($get('prodProcessLine.optional') &amp;&amp; priority &lt; $get('manufOrder.manufacturingOperationMaxPriority'))))"/>
    <button name="pauseBtn" icon="fa-pause"
      onClick="save,action-manufacturing-operation-group-pause,save" readonlyIf="statusSelect != 4"/>
    <button name="finishBtn" icon="fa-power-off"
      onClick="action-manufacturing-operation-group-finish" readonlyIf="statusSelect != 4"/>
    <button name="cancelBtn" icon="fa-times-circle"
      onClick="save,action-manufacturing-operation-group-cancel,save"
      readonlyIf="statusSelect == 2 || statusSelect == 6"/>
    <button name="printBtn" title="Print" icon="fa-print"
      onClick="save,action-method-print-birt"/>
  </grid>



  <form name="manufacturing-operation-form" title="Operation"
    model="com.axelor.apps.production.db.ManufacturingOperation"
    onLoad="action-manufacturing-operation-attrs-is-cons-pro-on-operation"
    onSave="action-manufacturing-operation-name"
    onNew="action-manufacturing-operation-attrs-is-cons-pro-on-operation,action-manufacturing-operation-default-record"
    canNew="false">
    <toolbar>
      <button name="printBtn" title="Print" icon="fa-print"
        onClick="action-method-print-birt"/>
    </toolbar>
    <panel name="mainPanel">
      <field name="statusSelect" showTitle="false" readonly="true" widget="NavSelect"
        colSpan="12"/>
      <field name="priority" colSpan="3" readonlyIf="statusSelect == 6"/>
      <field name="operationName" readonlyIf="statusSelect == 6" required="true"/>
      <field name="manufOrder" colSpan="3" hidden="true" readonly="true"/>
      <field name="prodProcessLine.outsourcable" hidden="true"/>
      <panel name="outsourcePanel" showTitle="false" colSpan="12">
        <field name="outsourcing" colSpan="4"
          hideIf="!prodProcessLine.outsourcable &amp;&amp; !prodProcessLine.outsourcing &amp;&amp; !manufOrder.outsourcing "
          readonlyIf="!prodProcessLine.outsourcable"/>
        <field name="outsourcingPartner"
          showIf="outsourcing &amp;&amp; !manufOrder.outsourcing"
          domain="self.isSubcontractor = true" colSpan="4"/>
      </panel>
      <field name="isToInvoice" colSpan="3"/>
      <field name="prodProcessLine" hidden="true"/>
      <field name="generatePurchaseOrderOnMoPlanning" hidden="true"/>
      <field name="prodProcessLine.optional" colSpan="3" readonly="true" showTitle="false">
        <viewer>
		  <![CDATA[<>{prodProcessLine.optional && <Badge style={{ backgroundColor: "#FC6355" }}>{_t('Optional')}</Badge>}</>]]>
        </viewer>
      </field>
      <field name="manufOrder.statusSelect" hidden="true"/>
      <field name="prodProcessLine.outsourcing" hidden="true"/>
      <field name="manufOrder.outsourcing" hidden="true"/>
      <field name="prodProcessLine.outsourcing" hidden="true"/>
      <field name="manufOrder.outsourcing" hidden="true"/>
    </panel>
    <panel name="datesPanel" title="Dates" colSpan="12" readonlyIf="statusSelect == 6">
      <panel name="plannedDates" title="Planned dates" colSpan="6">
        <field name="plannedStartDateT"
          onChange="action-manufacturing-operation-record-set-planned-end-date-time"/>
        <field name="plannedEndDateT"
          onChange="action-manufacturing-operation-record-set-planned-start-date-time-and-duration"/>
        <field name="plannedDuration" colSpan="12" widget="duration" x-seconds="true"
          x-big="true" onChange="action-manufacturing-operation-record-set-planned-end-date-time"/>
        <field name="plannedHumanDuration" colSpan="12" widget="duration" x-seconds="true"
          x-big="true" hideIf="workCenter.workCenterTypeSelect == 2"/>
        <field name="plannedMachineDuration" colSpan="12" widget="duration" x-seconds="true"
          x-big="true" hideIf="workCenter.workCenterTypeSelect == 1"/>
        <field name="workCenter.workCenterTypeSelect" hidden="true"/>
      </panel>
      <panel name="realDatesPanel" title="Real dates" colSpan="6">
        <field name="realStartDateT"
          onChange="save,action-manufacturing-operation-method-compute-duration,save"/>
        <field name="realEndDateT"
          onChange="save,action-manufacturing-operation-method-compute-duration,save"/>
        <field name="realDuration" colSpan="12" widget="duration" x-seconds="true"
          x-big="true"/>
      </panel>
    </panel>
    <panel name="resourcesPanel" title="Resources" colSpan="12" readonlyIf="statusSelect == 6">
      <field name="workCenter" grid-view="work-center-grid" form-view="work-center-form"
        colSpan="4" onChange="action-manufacturing-operation-attrs-workcenter-machine"
        onSelect="action-manufacturing-operation-attrs-set-work-center-domain"/>
      <field name="machine" hidden="true" readonly="true" grid-view="machine-grid"
        form-view="machine-form" colSpan="4"/>
      <field name="machineTool" domain="self.machine = machine" form-view="machine-tool-form"
        grid-view="machine-tool-grid" readonlyIf="statusSelect &gt; 3" colSpan="4"
        if="__config__.app.getApp('production')?.getEnableToolManagement()"/>
      <field name="prodProcessLine.description" colSpan="12"/>
      <field name="prodProcessLine.objectDescriptionList" colSpan="12" height="20"/>
    </panel>

    <panel name="stockMovesPanel" hidden="true" title="Stock moves" colSpan="12"
      readonlyIf="statusSelect == 6">
      <field name="inStockMoveList" form-view="stock-move-form" grid-view="stock-move-grid"
        colSpan="12"/>
      <panel hidden="true" name="consumedProductPanel" title="Consumed products" colSpan="12"
        readonlyIf="statusSelect == 6">
        <panel-related name="toConsumeProdProductListPanel"
          field="toConsumeProdProductList" readonlyIf="statusSelect &gt;= 3" colSpan="12"
          form-view="prod-product-form" grid-view="prod-product-grid"/>
        <panel-related name="consumedStockMoveLineListPanel"
          field="consumedStockMoveLineList"
          onChange="action-manufacturing-operation-method-check-consumed-stock-move,save,action-manufacturing-operation-method-update-consumed-stock-move"
          form-view="stock-move-line-production-form"
          grid-view="stock-move-line-consumed-production-grid" colSpan="12"/>
        <panel-related name="diffConsumeProdProductListPanel"
          field="diffConsumeProdProductList" colSpan="12" form-view="prod-product-form"
          grid-view="prod-product-grid"/>
      </panel>
    </panel>
    <panel name="actionsPanel" title="Actions" sidebar="true" colSpan="12" showIf="id"
      readonlyIf="statusSelect == 6">
      <field name="manufOrder.prodProcess" hidden="true"/>
      <field name="manufOrder.prodProcess.operationContinuity" hidden="true"/>
      <field name="manufOrder.manufacturingOperationMaxPriority" hidden="true"/>
      <button name="planBtn" title="Plan" icon="fa-calendar" colSpan="12"
        hideIf="statusSelect != 2 || $get('manufOrder.statusSelect') &lt; 3"
        onClick="save,action-manufacturing-operation-method-plan" hidden="true"/>
      <button name="startBtn" title="Start" icon="fa-play"
        showIf="(statusSelect == 3 &amp;&amp; !($get('manufOrder.prodProcess.operationContinuity'))) || (statusSelect == 3 &amp;&amp; ($get('manufOrder.prodProcess.operationContinuity') &amp;&amp; (!$get('prodProcessLine.optional') &amp;&amp; (priority == $get('manufOrder.manufacturingOperationMaxPriority'))))) || (statusSelect == 3 &amp;&amp; ($get('manufOrder.prodProcess.operationContinuity') &amp;&amp; ($get('prodProcessLine.optional') &amp;&amp; (priority &lt; $get('manufOrder.manufacturingOperationMaxPriority')))))"
        colSpan="12" onClick="save,action-manufacturing-operation-group-start,save" hidden="true"/>
      <button name="cancelBtn" title="Cancel" icon="fa-times-circle"
        showIf="statusSelect != 2 &amp;&amp; statusSelect != 6" colSpan="12"
        onClick="save,action-manufacturing-operation-group-cancel,save" hidden="true"/>
      <button name="finishBtn" title="Finish" icon="fa-power-off" showIf="statusSelect == 4"
        colSpan="12" onClick="save,action-manufacturing-operation-group-finish" hidden="true"/>
      <button name="finishPart" icon="fa-power-off" title="Partial Finish" colSpan="12"
        showIf="statusSelect == 4" onClick="save, action-manufacturing-operation-method-part-finish"
        hidden="true"/>
      <button name="pauseBtn" title="Pause" icon="fa-pause" showIf="statusSelect == 4"
        colSpan="12" onClick="save,action-manufacturing-operation-group-pause,save" hidden="true"/>
      <button name="resumeBtn" title="Resume" icon="fa-step-forward"
        showIf="(statusSelect == 5 &amp;&amp; !($get('manufOrder.prodProcess.operationContinuity'))) || (statusSelect == 5 &amp;&amp; ($get('manufOrder.prodProcess.operationContinuity') &amp;&amp; (!$get('prodProcessLine.optional') &amp;&amp; (priority == $get('manufOrder.manufacturingOperationMaxPriority'))))) || (statusSelect == 5 &amp;&amp; ($get('manufOrder.prodProcess.operationContinuity') &amp;&amp; ($get('prodProcessLine.optional') &amp;&amp; (priority &lt; $get('manufOrder.manufacturingOperationMaxPriority')))))"
        colSpan="12" onClick="save,action-manufacturing-operation-method-resume,save" hidden="true"/>
    </panel>

    <panel-related name="manufacturingOperationDurationListPanel"
      field="manufacturingOperationDurationList" canNew="false" canEdit="false" canRemove="false"
      sidebar="true" showIf="id"
      if="!__config__.app.getApp('production') || !__config__.app.getApp('production')?.getEnableTimesheetOnManufOrder()"/>

    <panel-related name="manufacturingOperationDurationListPanel"
      field="timesheetLineList" grid-view="timesheet-line-timesheet-manufacturing-operation-grid"
      form-view="timesheet-line-timesheet-manuf-order-form" colSpan="12" canNew="false"
      canEdit="false" canRemove="false" if-module="axelor-business-production"
      if="__config__.app.getApp('production')?.getEnableTimesheetOnManufOrder()"/>
  </form>

  <action-record name="action-manufacturing-operation-default-record"
    model="com.axelor.apps.production.db.ManufacturingOperation">
    <field name="statusSelect" expr="eval: 1"/>
    <field name="manufOrder" expr="eval: __parent__"/>
    <field name="outsourcing" expr="eval: __parent__?.outsourcing"/>
  </action-record>

  <action-record name="action-manufacturing-operation-name"
    model="com.axelor.apps.production.db.ManufacturingOperation">
    <field name="name" expr="eval: manufOrder?.manufOrderSeq + ' - ' + priority"/>
  </action-record>

  <action-group name="action-manufacturing-operation-group-start-or-resume">
    <action name="action-manufacturing-operation-method-start" if="statusSelect == 3"/>
    <action name="action-manufacturing-operation-method-resume" if="statusSelect == 5"/>
  </action-group>

  <action-group name="action-manufacturing-operation-group-start">
    <action name="action-manufacturing-operation-method-start"/>
    <action name="action-manufacturing-operation-method-compute-duration"/>
  </action-group>

  <action-group name="action-manufacturing-operation-group-finish">
    <action name="action-manufacturing-operation-method-finish"/>
    <action name="action-manufacturing-operation-method-compute-duration"/>
  </action-group>

  <action-method name="action-manufacturing-operation-method-pause">
    <call class="com.axelor.apps.production.web.ManufacturingOperationController"
      method="pause"/>
  </action-method>

  <action-group name="action-manufacturing-operation-group-pause">
    <action name="action-manufacturing-operation-method-pause"/>
  </action-group>

  <action-method name="action-manufacturing-operation-method-resume">
    <call class="com.axelor.apps.production.web.ManufacturingOperationController"
      method="resume"/>
  </action-method>

  <action-method name="action-manufacturing-operation-method-finish">
    <call class="com.axelor.apps.production.web.ManufacturingOperationController"
      method="finish"/>
  </action-method>

  <action-method name="action-manufacturing-operation-method-part-finish">
    <call class="com.axelor.apps.production.web.ManufacturingOperationController"
      method="partialFinish"/>
  </action-method>

  <action-method name="action-manufacturing-operation-method-cancel">
    <call class="com.axelor.apps.production.web.ManufacturingOperationController"
      method="cancel"/>
  </action-method>

  <action-group name="action-manufacturing-operation-group-cancel">
    <action name="action-manufacturing-operation-method-cancel"/>
  </action-group>

  <action-method name="action-manufacturing-operation-method-start">
    <call class="com.axelor.apps.production.web.ManufacturingOperationController"
      method="start"/>
  </action-method>

  <action-method name="action-manufacturing-operation-method-compute-duration">
    <call class="com.axelor.apps.production.web.ManufacturingOperationController"
      method="computeDuration"/>
  </action-method>

  <action-method
    name="action-manufacturing-operation-method-alert-if-planned-end-date-overflow">
    <call class="com.axelor.apps.production.web.ManufacturingOperationController"
      method="alertPlannedEndDateOverflow"/>
  </action-method>

  <action-method name="action-manufacturing-operation-method-set-planned-dates">
    <call class="com.axelor.apps.production.web.ManufacturingOperationController"
      method="setPlannedDates"/>
  </action-method>

  <action-group name="action-manufacturing-operation-group-set-planned-dates">
    <action name="action-manufacturing-operation-method-alert-if-planned-end-date-overflow"/>
    <action name="action-manufacturing-operation-method-set-planned-dates"/>
  </action-group>

  <action-method name="action-manufacturing-operation-method-set-real-dates">
    <call class="com.axelor.apps.production.web.ManufacturingOperationController"
      method="setRealDates"/>
  </action-method>

  <action-method name="action-manufacturing-operation-method-plan">
    <call class="com.axelor.apps.production.web.ManufacturingOperationController" method="plan"/>
  </action-method>

  <action-method
    name="action-manufacturing-operation-method-check-consumed-stock-move">
    <call class="com.axelor.apps.production.web.ManufacturingOperationController"
      method="checkConsumedStockMoveLineList"/>
  </action-method>

  <action-method
    name="action-manufacturing-operation-method-update-consumed-stock-move">
    <call class="com.axelor.apps.production.web.ManufacturingOperationController"
      method="updateConsumedStockMoveFromManufacturingOperation"/>
  </action-method>

  <action-attrs name="action-manufacturing-operation-attrs-is-cons-pro-on-operation">
    <attribute name="hidden" for="consumedProductPanel"
      expr="eval: !(manufOrder?.isConsProOnOperation || __parent__?.isConsProOnOperation)"/>
    <attribute name="hidden" for="stockMovesPanel"
      expr="eval: !(manufOrder?.isConsProOnOperation || __parent__?.isConsProOnOperation)"/>
    <attribute name="hidden" for="machine"
      expr="eval: workCenter?.workCenterTypeSelect == 1"/>
  </action-attrs>

  <action-attrs name="action-manufacturing-operation-attrs-workcenter-machine">
    <attribute for="machine" name="value" expr="eval: workCenter?.machine"/>
    <attribute name="hidden" for="machine"
      expr="eval: workCenter?.workCenterTypeSelect == 1"/>
    <attribute for="employeeSet" name="value" expr="eval: workCenter?.hrEmployeeSet"/>
  </action-attrs>

  <action-attrs name="action-manufacturing-operation-attrs-set-work-center-domain">
    <attribute name="domain" for="workCenter"
      expr="eval: 'self.id IN (' + prodProcessLine?.workCenterGroup?.workCenterSet?.collect{ it.id }.join(',') + ')'"
      if="prodProcessLine?.workCenterGroup?.workCenterSet &amp;&amp; !prodProcessLine?.workCenterGroup?.workCenterSet.isEmpty()"/>
  </action-attrs>

  <action-record
    name="action-manufacturing-operation-record-set-planned-start-date-time-and-duration"
    model="com.axelor.apps.production.db.ManufacturingOperation">
    <field name="plannedStartDateT"
      expr="eval: plannedEndDateT?.minusSeconds(plannedDuration)"
      if="plannedStartDateT != null &amp;&amp; plannedEndDateT != null &amp;&amp; plannedEndDateT.isBefore(plannedStartDateT)"/>
    <field name="plannedDuration"
      expr="eval: com.axelor.apps.tool.date.DurationTool.getSecondsDuration(com.axelor.apps.tool.date.DurationTool.computeDuration(plannedStartDateT, plannedEndDateT))"
      if="plannedStartDateT != null &amp;&amp; plannedEndDateT != null &amp;&amp; plannedEndDateT.isAfter(plannedStartDateT)"/>
  </action-record>

  <action-record
    name="action-manufacturing-operation-record-set-planned-end-date-time"
    model="com.axelor.apps.production.db.ManufacturingOperation">
    <field name="plannedEndDateT" expr="eval: plannedStartDateT?.plusSeconds(plannedDuration)"/>
  </action-record>

  <action-view name="action-manufacturing-view-on-click-planning-machine-calender-view"
    title="Machine Planning" model="com.axelor.apps.production.db.ManufacturingOperation">
    <view type="calendar" name="planning-machine-calender"/>
    <domain>self.statusSelect IN (3,4,5,6) AND self.machine.id = :_machineId</domain>
    <context name="_machineId" expr="eval: id"/>
  </action-view>

  <search-filters name="manufacturing-operation-filters"
    model="com.axelor.apps.production.db.ManufacturingOperation" title="Operation order filters">
    <field name="manufacturingOperationDurationList" hidden="true"
      if="__config__.app.getApp('production')?.getEnableTimesheetOnManufOrder()"/>
    <field name="timesheetLineList" hidden="true"
      if="!__config__.app.getApp('production')?.getEnableTimesheetOnManufOrder()"/>
    <filter name="manufacturing-operation-filters-draft-operations" title="Draft Operations">
      <domain>self.statusSelect = 1</domain>
    </filter>
    <filter name="manufacturing-operation-filters-planned-operations"
      title="Planned Operations">
      <domain>self.statusSelect = 3</domain>
    </filter>
    <filter name="manufacturing-operation-filters-operations-in-progress"
      title="Operations In Progress">
      <domain>self.statusSelect = 4</domain>
    </filter>
    <filter name="manufacturing-operation-filters-operations-in-standby"
      title="Operations In Standby">
      <domain>self.statusSelect = 5</domain>
    </filter>
    <filter name="manufacturing-operation-filters-finished-operations"
      title="Finished Operations">
      <domain>self.statusSelect = 6</domain>
    </filter>
    <filter name="manufacturing-operation-filters-cancelled-operations"
      title="Cancelled Operations">
      <domain>self.statusSelect = 2</domain>
    </filter>
    <filter name="manufacturing-operation-filters-operations-to-invoice"
      title="Operations to invoice">
      <domain>self.isToInvoice = true</domain>
    </filter>
    <filter name="manufacturing-operation-filters-late-planned-operations"
      title="Late Planned Operations">
      <domain>self.statusSelect = 3 AND self.plannedStartDateT &lt; :_todayDateTime</domain>
    </filter>
    <filter name="manufacturing-operation-filters-operations-finished-late"
      title="Operations Finished late">
      <domain>self.statusSelect = 6 AND self.plannedEndDateT &lt; self.realEndDateT</domain>
    </filter>
    <filter name="manufacturing-operation-filters-operations-with-difference"
      title="Operations with difference">
      <domain>self.isWithDifference = true</domain>
    </filter>
  </search-filters>
</object-views>

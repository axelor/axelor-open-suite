	<menuitem name="manufacturing-mps" parent="manufacturing-root" title="MPS" order="43" if="__config__.app.isApp('supplychain')"/>

	<menuitem name="manufacturing-mps-master-production-scheduling" parent="manufacturing-mps" title="Master Production Scheduling"

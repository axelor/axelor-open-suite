		<view type="form" name="operation-order-form" />
		<view-param name="search-filters" value="operation-order-filters"/>
		<context name="_todayDateTime" expr="eval:__config__.app.getTodayDateTime()"/>
	</action-view>	
	
	<menuitem name="manufacturing-root-operation-order-planned-calendar" parent="manufacturing-root"
		title="Planned operations" action="manufacturing.root.operation.order.planned.calendar" order="30" />

	<action-view name="manufacturing.root.operation.order.planned.calendar" title="Planned operations"
		model="com.axelor.apps.production.db.OperationOrder" >
		<view type="calendar" name="operation-order-planned-calendar"/>
		<view type="grid" name="operation-order-grid" />
		<view type="form" name="operation-order-form" />
		<domain>self.statusSelect = 3</domain>
	</action-view>
	
	<menuitem name="manufacturing-root-operation-order-real-calendar" parent="manufacturing-root"
		title="Real operations" action="manufacturing.root.operation.order.real.calendar" order="35" />

	<action-view name="manufacturing.root.operation.order.real.calendar" title="Real operations"
		model="com.axelor.apps.production.db.OperationOrder" >
		<view type="calendar" name="operation-order-real-calendar"/>
		<view type="grid" name="operation-order-grid" />
		<view type="form" name="operation-order-form" />
		<domain>self.statusSelect = 6</domain>
	</action-view>
	

	<menuitem name="manufacturing-root-outsourcing" title="Outsourcing" parent="manufacturing-root" order="37" if="__config__.app.getApp('production').getManageOutsourcing()"/>

	<menuitem name="manufacturing-root-outsourcing-subcontractors" title="Subcontractors" parent="manufacturing-root-outsourcing" action="manufacturing.root.subcontractors"/>
	<action-view name="manufacturing.root.subcontractors" title="Subcontractors" model="com.axelor.apps.base.db.Partner">
		<view type="cards" name="partner-cards"/>
		<view type="grid" name="partner-grid" />
		<view type="form" name="partner-form" />
		<domain>self.isSubcontractor = :_isSubcontractor</domain>
		<context name="_isSubcontractor" expr="eval:true"/>
	</action-view>

	<menuitem name="manufacturing-root-outsourcing-subcontractors-deliveries" title="Subcontractors deliveries"
		parent="manufacturing-root-outsourcing" action="manufacturing.root.subcontractors.deliveries"/>
	<action-view name="manufacturing.root.subcontractors.deliveries" title="Subcontractors deliveries" model="com.axelor.apps.stock.db.StockMove">
		<view type="grid" name="stock-move-out-grid"/>
        <view type="calendar" name="stock-move-calendar" />
		<view type="form" name="stock-move-customer-deliveries-form"/>
        <domain>self.typeSelect = :_typeSelect AND
        		self.isReversion = FALSE AND
        		self.toStockLocation.typeSelect=:_stockLocTypeSelect AND
        		self.toStockLocation.isOutsourcingLocation=:_isOutsourcingLoc</domain>
		<context name="_typeSelect" expr="eval: __repo__(StockMove).TYPE_OUTGOING"/>
		<context name="_stockLocTypeSelect" expr="eval: __repo__(StockLocation).TYPE_VIRTUAL"/>
		<context name="_isOutsourcingLoc" expr="eval:true"/>
	</action-view>

	<menuitem name="manufacturing-root-outsourcing-subcontractors-arrivals" title="Subcontractors arrivals"
		parent="manufacturing-root-outsourcing" action="manufacturing.root.subcontractors.arrivals"/>
	<action-view name="manufacturing.root.subcontractors.arrivals" title="Subcontractors arrivals" model="com.axelor.apps.stock.db.StockMove">
		<view type="grid" name="stock-move-in-grid"/>
        <view type="calendar" name="stock-move-calendar" />
		<view type="form" name="stock-move-supplier-arrivals-form"/>
        <domain>self.typeSelect = :_typeSelect AND
        		self.isReversion = FALSE AND
        		self.fromStockLocation.typeSelect=:_stockLocTypeSelect AND
        		self.fromStockLocation.isOutsourcingLocation=:_isOutsourcingLoc</domain>
		<context name="_typeSelect" expr="eval: __repo__(StockMove).TYPE_INCOMING"/>
		<context name="_stockLocTypeSelect" expr="eval: __repo__(StockLocation).TYPE_VIRTUAL"/>
		<context name="_isOutsourcingLoc" expr="eval:true"/>
	</action-view>



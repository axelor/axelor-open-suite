		<view type="form" name="mrp-manuf-order-form"/>
		<domain>self.mrpTypeSelect = 2</domain>
	</action-view>

	<menuitem name="manufacturing-mps-forecast" parent="manufacturing-mps" title="Forecasts" action="manufacturing.mps.forecast" order="2"/>


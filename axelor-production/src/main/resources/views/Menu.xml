	<action-view name="manufacturing.mps.conf.manufacturing-periods" title="Manufacturing periods" model="com.axelor.apps.base.db.Year">
		<view type="grid" name="year-account-grid"/>
		<view type="form" name="year-manuf-order-form"/>
		<domain>self.typeSelect = 3</domain>
	</action-view>

	<menuitem name="manufacturing-root-sop" parent="manufacturing-root"
		title="S&amp;OP" action="manufacturing.root.sop" order="46"/>

	<action-view name="manufacturing.root.sop" title="Sales and Operations Planning"
		model="com.axelor.apps.production.db.Sop">
		<view type="grid" name="sop-grid"/>
		<view type="form" name="sop-form"/>
	</action-view>

	<menuitem name="manufacturing-root-mps-charge" parent="manufacturing-root"
		title="MPS Charge" action="manufacturing.root.mps.charge" order="43"
		if="__config__.app.getApp('production')?.manageMpsCharge"/>

 	<action-view name="manufacturing.root.mps.charge" title="Master Production Scheduling Charge"

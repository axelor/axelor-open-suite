    Metafile.file_path AS "logo_path",
    string_agg(StockLocation.name, ', ') AS "stockLocationNames",
    RawMaterialRequirement.duration,
    RawMaterialRequirement.show_all_requirements,
    RawMaterialRequirement.show_requirements_bymo
FROM
	production_raw_material_requirement RawMaterialRequirement
LEFT JOIN
    base_company Company
    ON (Company.id = RawMaterialRequirement.company)
LEFT JOIN
    meta_file Metafile
    ON (Metafile.id = Company.logo)
LEFT JOIN
    production_raw_material_requirement_stock_location_set StockLocationSet
    ON StockLocationSet.production_raw_material_requirement = RawMaterialRequirement.id
LEFT JOIN
    stock_stock_location StockLocation
    ON StockLocation.id = StockLocationSet.stock_location_set
WHERE
    RawMaterialRequirement.id = ?
GROUP BY RawMaterialRequirement.code,RawMaterialRequirement.name, Metafile.file_path, RawMaterialRequirement.duration, Company.name,
         RawMaterialRequirement.show_all_requirements, RawMaterialRequirement.show_requirements_bymo
]]></xml-property>

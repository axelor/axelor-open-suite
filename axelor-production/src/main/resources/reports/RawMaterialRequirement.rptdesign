            <xml-property name="queryText"><![CDATA[SELECT
    Product.full_name as "product",
    ProductionStockLocation.name as "stockLocation",
    ManufOrder.manuf_order_seq,
    CASE ManufOrder.is_cons_pro_on_operation
      WHEN true THEN sum(StockMoveLine.qty)
      ELSE
        (CASE count(OperationOrder.id)
            WHEN 0 THEN sum(StockMoveLine.qty)
            ELSE sum(StockMoveLine.qty)/count(OperationOrder.id)
        END)
    END as "neededQty",
    (CASE ManufOrder.is_cons_pro_on_operation
      WHEN true THEN sum(StockMoveLine.qty)
      ELSE
        (CASE count(OperationOrder.id)
            WHEN 0 THEN sum(StockMoveLine.qty)
            ELSE sum(StockMoveLine.qty)/count(OperationOrder.id)
        END)
    END) - ProductionStockLocationLine.current_qty as "missingNegativeQty",
    StockLocation.name as "availableStockLocation",
    sum(StockLocationLine.current_qty)/count(StockMoveLine.qty) as "availableQty",
    Unit.label_to_printing
FROM
    production_raw_material_requirement RawMaterialRequirement
LEFT JOIN
    base_company Company
    on Company.id = RawMaterialRequirement.company
LEFT JOIN
    production_manuf_order ManufOrder
    on ManufOrder.company = Company.id
LEFT JOIN
	production_prod_process ProdProcess
	on ProdProcess.id = ManufOrder.prod_process
LEFT JOIN 
	stock_stock_location ProductionStockLocation
	on ProdProcess.stock_location = ProductionStockLocation.id
LEFT JOIN
	stock_stock_location_line ProductionStockLocationLine
	on ProductionStockLocationLine.stock_location = ProductionStockLocation.id
LEFT JOIN
    production_operation_order OperationOrder on OperationOrder.manuf_order = ManufOrder.id
LEFT JOIN
    stock_stock_move StockMove on (
    (ManufOrder.is_cons_pro_on_operation = false OR ManufOrder.is_cons_pro_on_operation IS NULL)
            AND StockMove.in_manuf_order = ManufOrder.id)
        OR (ManufOrder.is_cons_pro_on_operation = true
            AND StockMove.in_operation_order = OperationOrder.id)
LEFT JOIN
    stock_stock_move_line StockMoveLine on StockMoveLine.stock_move = StockMove.id
LEFT JOIN
    base_product Product
    on StockMoveLine.product = Product.id
LEFT JOIN
	base_unit Unit on Product.unit = Unit.id
LEFT JOIN
    production_raw_material_requirement_stock_location_set StockLocationSet
    on StockLocationSet.production_raw_material_requirement = RawMaterialRequirement.id
LEFT JOIN
    stock_stock_location StockLocation
    on StockLocation.id = StockLocationSet.stock_location_set
LEFT JOIN
    stock_stock_location_line StockLocationLine
    on StockLocationLine.stock_location = StockLocation.id
WHERE
    StockLocationLine.product = Product.id
    AND ProductionStockLocationLine.product = Product.id
    AND StockMove.status_select = 2
    AND (CURRENT_DATE + RawMaterialRequirement.duration) >= StockMove.estimated_date
    AND RawMaterialRequirement.id = ?
GROUP BY
    Product.full_name,
    ProductionStockLocation.name,
    ManufOrder.manuf_order_seq,
    ManufOrder.is_cons_pro_on_operation,
    ProductionStockLocationLine.current_qty,
    label_to_printing,
    StockLocation.name;
]]></xml-property>

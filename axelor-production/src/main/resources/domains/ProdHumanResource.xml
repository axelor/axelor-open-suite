<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_6.1.xsd">

  <module name="production" package="com.axelor.apps.production.db"/>

  <entity name="ProdHumanResource">

    <many-to-one name="workCenter" ref="com.axelor.apps.production.db.WorkCenter"
      title="Work center"/>
    <many-to-one name="product" ref="com.axelor.apps.base.db.Product" title="Profile"
      initParam="true"/>
    <long name="duration" title="Duration" initParam="true"/>
    <many-to-one name="costSheetGroup"
      ref="com.axelor.apps.production.db.CostSheetGroup" title="Cost sheet group"/>
    <many-to-one name="operationOrder"
      ref="com.axelor.apps.production.db.OperationOrder" title="Operation order"/>

  </entity>
</domain-models>

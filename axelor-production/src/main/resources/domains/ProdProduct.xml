<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.2.xsd">

  <module name="production" package="com.axelor.apps.production.db"/>

  <entity name="ProdProduct" lang="java">

	<many-to-one name="product" ref="com.axelor.apps.base.db.Product" title="Product" initParam="true"/>  	
  	<decimal name="qty" title="Qty" initParam="true" precision="20" scale="10"/>
  	<many-to-one name="unit" ref="com.axelor.apps.base.db.Unit" title="Unit" initParam="true"/>
  	
<!--   	Contreparties - Non affichÃ©  -->
<!-- 	Operation order -->
  	<many-to-one name="toConsumeOperationOrder" ref="com.axelor.apps.production.db.OperationOrder"/>
  	<many-to-one name="diffConsumeOperationOrder" ref="com.axelor.apps.production.db.OperationOrder"/>
  	
<!--   	Manuf order -->
  	<many-to-one name="toConsumeManufOrder" ref="com.axelor.apps.production.db.ManufOrder"/>
  	<many-to-one name="diffConsumeManufOrder" ref="com.axelor.apps.production.db.ManufOrder"/>
  	
  	<many-to-one name="toProduceManufOrder" ref="com.axelor.apps.production.db.ManufOrder"/>
  	<many-to-one name="wasteManufOrder" ref="com.axelor.apps.production.db.ManufOrder" title="MO"/>
  	
<!--   	Prod process line -->
  	<many-to-one name="toConsumeProdProcessLine" ref="com.axelor.apps.production.db.ProdProcessLine"/>

    <!--Used for showing diff-->
		<decimal name="plannedQty" title="Planned Qty" precision="20" scale="10"/>
		<decimal name="realQty" title="Real Qty" precision="20" scale="10"/>

  	
  </entity>
</domain-models>

<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.3.xsd">

    <module name="production" package="com.axelor.apps.production.db"/>

    <entity name="OperationOrderDuration" lang="java">
        <many-to-one name="operationOrder" ref="com.axelor.apps.production.db.OperationOrder" title="Operation Order"/>

        <many-to-one name="startedBy" ref="com.axelor.auth.db.User" title="Started by" readonly="true"/>
        <datetime name="startingDateTime" title="Starting Date" readonly="true"/>

        <many-to-one name="stoppedBy" ref="com.axelor.auth.db.User" title="Stopped by" readonly="true"/>
        <datetime name="stoppingDateTime" title="Stopping Date" readonly="true"/>
    </entity>
</domain-models>
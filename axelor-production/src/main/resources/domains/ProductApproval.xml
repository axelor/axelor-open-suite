<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_6.1.xsd">

  <module name="production" package="com.axelor.apps.production.db"/>

  <entity name="ProductApproval" cacheable="true">

    <string name="code" title="Code" unique="true" required="true"/>
    <string name="name" title="name" namecolumn="true" unique="true" required="true"/>
    <string name="description" title="Description" large="true"/>
    <boolean name="controlOnReceipt" title="Control on receipt"/>

  </entity>

</domain-models>

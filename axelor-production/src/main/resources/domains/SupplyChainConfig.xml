<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_6.1.xsd">

  <module name="supplychain" package="com.axelor.apps.supplychain.db"/>

  <entity name="SupplyChainConfig">

    <boolean name="autoRequestReservedQtyOnManufOrder"
      title="Auto request qty for manuf orders"
      help="If true, the components will be automatically requested for reservation on planning manufacturing orders."/>

    <track>
      <field name="autoRequestReservedQtyOnManufOrder" on="UPDATE"/>
    </track>

  </entity>

</domain-models>

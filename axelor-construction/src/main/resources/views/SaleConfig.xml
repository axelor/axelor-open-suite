<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.0.xsd">


  <form id="construction-sale-config-form" name="sale-config-form" title="Sale configuration"
    model="com.axelor.apps.sale.db.SaleConfig" onNew="action-sale-config-record-default"
    width="large" extension="true">
    <extend target="//field[@name='defaultHoldBackPercentage']">
      <insert position="after">
        <field name="defaultGrossMarging"/>
      </insert>
    </extend>
  </form>

</object-views>

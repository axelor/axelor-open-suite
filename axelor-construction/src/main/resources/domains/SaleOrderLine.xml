<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_7.0.xsd">

  <module name="sale" package="com.axelor.apps.sale.db"/>

  <entity name="SaleOrderLine" implements="com.axelor.apps.base.interfaces.PricedOrderLine">
    <decimal name="purchasePrice" title="Purchase price"/>
    <decimal name="costPrice" title="Cost price"/>
    <decimal name="generalExpenses" title="General expenses" default="1"/>
    <decimal name="grossMarging" title="Gross margin" default="0"/>
    <many-to-one name="productType" title="Product type"
      ref="com.axelor.apps.base.db.ProductType"/>
  </entity>

</domain-models>
